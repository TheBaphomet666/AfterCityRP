CMD:sirenadp(playerid, params[])
{
	new veh, type[8], string[128];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
   	if(!IsACop(playerid) && !IsADOC(playerid) && !IsASAAF(playerid) && !IsFBI(playerid) && !IsSASD(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No eres un agente de LSPD/FBI.");
   	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_GREY, "No estás conduciendo un vehiculo.");
	if(sscanf(params, "s[8]", type))
	{
		SendClientMessage(playerid, COLOR_WHITE, "USO: /sirenapd [posición]");
		SendClientMessage(playerid, COLOR_WHITE, "Posiciones: dentro | techo | apagado");
		return 1;
	}
	if(Siren[veh]) return SendClientMessage(playerid, COLOR_GREY, "Este vehículo ya tiene una sirena conectada.");
	veh = GetPlayerVehicleID(playerid);
	if(!strcmp(type, "dentro"))
	{
		if(Siren[veh]) return SendClientMessage(playerid, COLOR_GREY, "Ya hay una sirena conectada a este vehículo.");
        Siren[veh] = 1;
        SirenObject[veh] = CreateDynamicObject(18646, 10.0, 10.0, 10.0, 0, 0, 0);
        AttachDynamicObjectToVehicle(SirenObject[veh], veh, 0.0, 0.75, 0.275, 0.0, 0.1, 0.0); // Inside - Premier
        format(string, sizeof(string), "* %s fija una sirena en el salpicadero.", RPN(playerid));
        SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
        return 1;
	}
	else if(!strcmp(type, "techo"))
	{
	    if(Siren[veh]) return SendClientMessage(playerid, COLOR_GREY, "Ya hay una sirena conectada a este vehículo.");
		Siren[veh] = 2;
        SirenObject[veh] = CreateDynamicObject(18646, 10.0, 10.0, 10.0, 0, 0, 0);
        AttachDynamicObjectToVehicle(SirenObject[veh], veh, -0.43, 0.0, 0.785, 0.0, 0.1, 0.0);
        format(string, sizeof(string), "* %s fija una sirena en el techo del vehículo.", RPN(playerid));
        SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
        return 1;
	}
	else if(!strcmp(type, "apagado"))
	{
	    if(!Siren[veh]) return SendClientMessage(playerid, COLOR_GREY, "No hay sirena conectada a este vehículo.");
		Siren[veh] = 0;
		DestroyDynamicObject(SirenObject[veh]);
		format(string, sizeof(string), "* %s toma la sirena de nuevo.", RPN(playerid));
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
		return 1;
	}
	return 1;
}

CMD:revisarveh(playerid, params[])
{
	new veh, string[128], playerb;
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
   	if(!IsACop(playerid) && !IsADOC(playerid) && !IsASAAF(playerid) && !IsFBI(playerid) && !IsSASD(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No eres un agente de LSPD/FBI.");
	if(sscanf(params, "i", veh)) return SendClientMessage(playerid, COLOR_GREY, "USO: /revisarveh [vehicleid]");
	if(veh == INVALID_VEHICLE_ID) return SendClientMessage(playerid, COLOR_GREY, "ID de vehículo no válido.");
	if(!IsPlayerNearVehicle(playerid, veh, 4)) return SendClientMessage(playerid, COLOR_GREY, "Usted no está cerca de este vehículo.");
    if(!IsTruckerVehicle(veh)) return SendClientMessage(playerid, COLOR_GREY, "Este no es un vehículo de camiones.");
    if(TruckPackages[veh] == -1) return SendClientMessage(playerid, COLOR_GREY, "Este camión no tiene paquetes dentro de él.");
    playerb = TruckPackages[veh];
	format(string, sizeof(string), "* %s comprueba la carga y la capacidad del camión.", RPN(playerid));
 	SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	if(PlayerInfo[playerb][pTPackages] == 1) SendClientMessage(playerid, COLOR_LIME, " Este camión no está sobrecargado con paquetes.");
	else if(PlayerInfo[playerb][pTPackages] == 2) SendClientMessage(playerid, COLOR_LIGHTRED, " Este camión está sobrecargado de paquetes.");
	return 1;
}

CMD:arrastrar(playerid, params[])
{
	new playerb, string[128];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_GREY, "USO: /arrastrar [playerid]");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes arrastrarte.");
	if(Drag[playerid] != -1 || Dragged[playerid] != -1) return SendClientMessage(playerid, COLOR_GREY, "No puedes hacer eso en este momento.");
    if(!IsPlayerNearPlayer(playerid, playerb, 2)) return SendClientMessage(playerid, COLOR_GREY, "Estás muy lejos de ese jugador.");
	if(IsPlayerInAnyVehicle(playerb)) return SendClientMessage(playerid, COLOR_GREY, "No se puede arrastrar a alguien desde un vehículo.");
	if(Dragged[playerb] != -1) return SendClientMessage(playerid, COLOR_GREY, "El jugador ya está siendo arrastrado.");
	Drag[playerid] = playerb;
	Dragged[playerb] = playerid;
	format(string, sizeof(string), "* %s tira a %s y empieza a arrastrarlos.", RPN(playerid), RPN(playerb));
 	SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
 	SendClientMessage(playerid, COLOR_GREY, "Recuerda /darrastrar luego de que hayas terminado de arrastrar el reproductor.");
	return 1;
}

