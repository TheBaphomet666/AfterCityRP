CMD:curar(playerid, params[])
{

	new giveplayerid, price;
	if(sscanf(params, "ud", giveplayerid, price)) return SendClientMessage(playerid, COLOR_GREY, "USO: /curar [id] [precio]");

	if(!(200 <= price <= 1000))
	{
		SendClientMessage(playerid, COLOR_GREY, "El precio va entre 200 y 1000.");
		return 1;
	}
	if (giveplayerid == playerid)
	{
		SendClientMessage(playerid, COLOR_GREY, "No te puedes curar a ti mismo.");
		return 1;
	}
	if (IsPlayerConnected(giveplayerid))
	{
		if(IsMedic(playerid))//model
		{
			
			    new Float:X, Float:Y, Float:Z;
	   			GetPlayerPos(giveplayerid, X, Y, Z);

				if(!IsPlayerInRangeOfPoint(playerid, 10, X, Y, Z)) return SendClientMessage(playerid, COLOR_GREY,"No estás cerca de esa persona");
				new Float:tempheal;
				GetPlayerHealth(giveplayerid,tempheal);
				if(tempheal >= 100.0)
				{
					SendClientMessage(playerid, COLOR_GREEN,"Esa persona está a full vida.");
					return 1;
				}
				new string[64];
				format(string, sizeof(string), "Curaste a %s por $%d.", RPN(giveplayerid),price);
				SendClientMessage(playerid, COLOR_RED, string);
				GiveDodMoney(playerid, price / 2);
				TaxMoney += price / 2;
				GiveDodMoney(giveplayerid, -price);
				SetPlayerHealth(giveplayerid, 100);
				PlayerPlaySound(playerid, 1150, 0.0, 0.0, 0.0);
				PlayerPlaySound(giveplayerid, 1150, 0.0, 0.0, 0.0);
				PlayerInfo[giveplayerid][pDead] = 0;
				TogglePlayerControllable(giveplayerid, 1);
				ApplyAnimation(giveplayerid,"PED","WALK_player",4.1,1,1,1,1,1);
				format(string, sizeof(string), "Has sido curado por $%d por %s.",price, RPN(playerid));
				SendClientMessage(giveplayerid, COLOR_GREEN,string);

		}
		else
		{
			SendClientMessage(playerid, COLOR_GREY, "No estás autorizado.");
			return 1;
		}
	}
	else
	{
		SendClientMessage(playerid, COLOR_GREY, "Id inválido.");
	}
	return 1;
}

