public OnPlayerExitVehicle(playerid, vehicleid)
{
    if(GetPVarInt(playerid, "FPS_On") == 1)
	{
		SetPVarInt(playerid, "FPS_On", 0);
		SetCameraBehindPlayer(playerid);
		DestroyPlayerObject(playerid, FPSObject[playerid]);
	}
	if(g_bIsRobbiz[playerid])
	{
		g_robbizBag[playerid] = CreateObject(1550, 0.0, 0.0, 1000.0, 0.0, 0.0, 0.0);
		AttachObjectToPlayer(g_robbizBag[playerid], playerid, -0.2, -0.4, 0.0, 0.0, 1.5, 2);
	}
	if(LicenseTest[playerid])
	{
		DisablePlayerCheckpoint(playerid);
		LicenseTest[playerid] = 0;
		CP[playerid] = 0;
		SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		SendClientMessage(playerid, COLOR_WHITE, "* Licenciatura: Usted ha dejado su coche, por lo tanto su prueba ha sido fallida.");
	}
	if(IsACop(playerid) || IsNG(playerid) || IsFBI(playerid) || IsSASD(playerid))
	{
		LastCar[playerid] = vehicleid;
	}
	if(PlayerInfo[playerid][pSeatbelt] == 1 && !IsABike(GetPlayerVehicleID(playerid)))
    {
        PlayerInfo[playerid][pSeatbelt] = 0;
  		SendClientMessage(playerid, COLOR_WHITE, "* Has desabrochado tu cinturón de seguridad");
   	}
    else if(PlayerInfo[playerid][pSeatbelt] == 1 && IsABike(GetPlayerVehicleID(playerid)))
 	{
		PlayerInfo[playerid][pSeatbelt] = 0;
		SendClientMessage(playerid, COLOR_WHITE, "* Te has quitado tu casco");
  		RemovePlayerAttachedObject(playerid, 9);
	}
	if(PlayerInfo[playerid][pRadio] == 1)
	{
	    StopAudioStreamForPlayer(playerid);
	    PlayerInfo[playerid][pRadio] = 0;
	}
	return 1;
}

public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if(newstate == PLAYER_STATE_PASSENGER)
	{
	    SetPlayerArmedWeapon(playerid,0);
	}
	if(newstate == PLAYER_STATE_PASSENGER)
	{
	    if(newstate == PLAYER_STATE_PASSENGER)
	    {
	        new gun,tmp;
	        GetPlayerWeaponData(playerid,4,gun,tmp);
	        #pragma unused tmp
	        if(gun)SetPlayerArmedWeapon(playerid,gun);
	        else SetPlayerArmedWeapon(playerid,0);
	    }
	}
	if(newstate == PLAYER_STATE_DRIVER && oldstate != PLAYER_STATE_DRIVER)
	{
	    new vehicleid = GetPlayerVehicleID(playerid);
	    new engine, lights, alarm, doors, bonnet, boot, objective;
