CMD:famedit(playerid, params[])
{
	new string[128], option[32], idx;
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pFamRank] < 5) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	idx = PlayerInfo[playerid][pFam];
	if(sscanf(params, "s[32]", option))
	{
		SendClientMessage(playerid, COLOR_WHITE, "USO: /famedit [opcion]");
		SendClientMessage(playerid, COLOR_GREY, "Opciones: rangos | safe | motd");
		return 1;
	}
    if(!strcmp(option, "cnombre", true, 3))
    {
	    if(sscanf(params, "s[32]s[32]", option, params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /famedit cname [nombre]");
		format(FamInfo[idx][fName], 32, "%s", params);
		format(string, sizeof(string), " Has cambiado el nombre de tu familia a {33AA33}%s", params);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "Caja fuerte de %s", FamInfo[idx][fName]);
    	UpdateDynamic3DTextLabelText(FamInfo[idx][fText], COLOR_WHITE, string);
	}
    else if(!strcmp(option, "rangos", true, 4))
    {
        new rank;
    	if(sscanf(params, "s[32]is[32]", option, rank, params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /famedit [rango] [nombre]");
		if(rank < 0 || rank > 6) return SendClientMessage(playerid, COLOR_GREY, "Los rangos de familia son entre 1 y 6");
		switch(rank)
		{
		    case 1: format(FamInfo[idx][fRank1], 32, "%s", params);
		    case 2: format(FamInfo[idx][fRank2], 32, "%s", params);
		    case 3: format(FamInfo[idx][fRank3], 32, "%s", params);
			case 4: format(FamInfo[idx][fRank4], 32, "%s", params);
			case 5: format(FamInfo[idx][fRank5], 32, "%s", params);
			case 6: format(FamInfo[idx][fRank6], 32, "%s", params);
		}
		format(string, sizeof(string), " Has cambiado el nombre del rango %d a {33AA33}%s", rank, params);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
    }
   	else if(!strcmp(option, "safe", true, 3))
    {
        new confirm[8];
    	if(PlayerInfo[playerid][pFamRank] < 6) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
        if(FamInfo[idx][fSafeX] != 0 && FamInfo[idx][fSafeY] != 0 && FamInfo[idx][fSafeZ] != 0)
		{
		    // SSCANF Check
		    if(sscanf(params, "s[32]s[8]", option, confirm))
		    {
		    	SendClientMessage(playerid, COLOR_WHITE, "USO: /famedit safe [confirmar]");
		    	SendClientMessage(playerid, COLOR_LIGHTRED, " Cambiar el ubicación de la caja fuerte/HQ costará $50,000.");
		    	return 1;
		    }
		    // Confirm Check
		    if(strcmp(confirm, "confirm", true, 8))
		    {
		    	SendClientMessage(playerid, COLOR_WHITE, "USO: /famedit safe [confirmar]");
		    	SendClientMessage(playerid, COLOR_LIGHTRED, " Cambiar el ubicación de la caja fuerte/HQ costará $30,000.");
		    	return 1;
		    }
		    // Money Check
		    if(PlayerInfo[playerid][pMoney] < 30000) return SendClientMessage(playerid, COLOR_GREY, "No tienes el dinero suficiente. ($30,000)");
		    GiveDodMoney(playerid, -30000);
		}
		GetPlayerPos(playerid,FamInfo[idx][fSafeX], FamInfo[idx][fSafeY], FamInfo[idx][fSafeZ]);
        DestroyDynamicPickup(FamInfo[idx][fPickup]);
        FamInfo[idx][fsafevw] = GetPlayerVirtualWorld(playerid);
	    FamInfo[idx][fPickup] = CreateDynamicPickup(1239, 1, FamInfo[idx][fSafeX], FamInfo[idx][fSafeY], FamInfo[idx][fSafeZ],FamInfo[idx][fsafevw]);
		DestroyDynamic3DTextLabel(FamInfo[idx][fText]);
		format(string, sizeof(string), "%s fseguro", FamInfo[idx][fName]);
    	FamInfo[idx][fText] = Create3DTextLabel(string, COLOR_WHITE, FamInfo[idx][fSafeX], FamInfo[idx][fSafeY], FamInfo[idx][fSafeZ]+0.3, 15, FamInfo[idx][fsafevw]);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has cambiado la ubicación de la caja fuerte/HQ.");
    }
    else if(!strcmp(option, "motd", true, 3))
    {
		if(sscanf(params, "s[32]s[32]", option, params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /famedit motd [motd]");
		format(FamInfo[idx][fMOTD], 64, "%s", params);
		format(string, sizeof(string), "Motd Familiar: {FFFFFF}%s", FamInfo[PlayerInfo[playerid][pFam]][fMOTD]);
   		SendPlayerFamMessage(playerid, COLOR_LIGHTRED, string);
    }
    else SendClientMessage(playerid, COLOR_GREY, "Opcion Inválida.");
	return 1;
}

