public OnGameModeExit()
{
    /*IRC_Quit(botIDs[0], "Gamemode quit");
	IRC_Quit(botIDs[1], "Gamemode quit");
	IRC_Quit(botIDs[2], "Gamemode quit");
	IRC_Quit(botIDs[3], "Gamemode quit");
	IRC_DestroyGroup(groupID); */
	foreach(Player, i)
    {
       	SaveChar(i);
       	ClearChar(i);
    }
	SaveFactions();
	SaveFamilies();
	SavePoints();
	SaveBiz();
	SaveHouses();
	SaveGarages();
	SaveDoors();
	SaveObj();
	SaveGates();
	SaveMapIcons();
	SaveArrestPoint();
	SaveArrestPoint2();
	SaveDrugs();
	SaveProducts();
	SaveMOTDS();
	SaveServerStats();
	KillTimer(FlashTimer);
	DestroyDynamicObject(FireObject);
	DestroyDynamicObject(SmokeObject);
	FireHealth = 0;
 	for (new i = 0; i < MAX_VEHICLES; i++)
  	{
   		DestroyObject(obj1[i]);
     	DestroyObject(obj2[i]);
  	}
	RemoveCameras();
	print("Todas las camaras han sido removidas.");
	return 1;
}

