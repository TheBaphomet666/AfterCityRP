CMD:dedit(playerid, params[])
{
	new idx, text[128], string[128];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pAdmin] < 4 && !PlayerInfo[playerid][pGangMod]) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "s[128]", params))
	{
		SendClientMessage(playerid, COLOR_WHITE, "USO: /dedit [option] [doorid]");
		SendClientMessage(playerid, COLOR_GREY, "OPTIONS: Pickup | Texto | Interior | Exterior | CustomInterior | CustomExterior");
		return 1;
	}
	if(!strcmp(params, "pickup", true, 6))
	{
	    new pickup;
	    if(sscanf(params, "s[128]ii", params, idx, pickup)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /dedit pickup [doorid] [pickupid]");
        if(DoorInfo[idx][dType] == 0) return SendClientMessage(playerid, COLOR_GREY, "Invalida PuertaID.");
		DoorInfo[idx][dType] = pickup;
		if(IsValidDynamicPickup(DoorInfo[idx][dPickup]))
		{
		    DestroyDynamicPickup(DoorInfo[idx][dPickup]);
		}
		DoorInfo[idx][dPickup] = CreateDynamicPickup(DoorInfo[idx][dType], 1, DoorInfo[idx][dOX], DoorInfo[idx][dOY], DoorInfo[idx][dOZ], DoorInfo[idx][dOVW], DoorInfo[idx][dOInt]);
	    format(string, sizeof(string), " You have set door ID %d's pickup to %d", idx, pickup);
	    SendClientMessage(playerid, COLOR_WHITE, string);
	}
	if(!strcmp(params, "texto", true, 4))
	{
	    if(sscanf(params, "s[128]is[128]", params, idx, text)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /dedit text [doorid] [text]");
	    if(AntiAdv(playerid, params)) return 1;
        if(DoorInfo[idx][dType] == 0) return SendClientMessage(playerid, COLOR_GREY, "Invalid door id.");
		format(DoorInfo[idx][dText], 128, "%s", text);
		format(string, sizeof(string), "ID: %d\n%s", idx, DoorInfo[idx][dText]);
		UpdateDynamic3DTextLabelText(DoorInfo[idx][dTextID], COLOR_WHITE, string);
	    format(string, sizeof(string), " Ha configurado el ID de la puerta %d en sus coordenadas interiores. %s", idx, text);
	    SendClientMessage(playerid, COLOR_WHITE, string);
	}
	if(!strcmp(params, "interior", true, 8))
	{
	    if(sscanf(params, "s[128]i", params, idx, text)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /dedit interior [doorid]");
        if(DoorInfo[idx][dType] == 0) return SendClientMessage(playerid, COLOR_GREY, "Invalida puerta id.");
		DoorInfo[idx][dIInt] = GetPlayerInterior(playerid);
		DoorInfo[idx][dIVW] = GetPlayerVirtualWorld(playerid);
		GetPlayerFacingAngle(playerid, DoorInfo[idx][dIA]);
		GetPlayerPos(playerid, DoorInfo[idx][dIX], DoorInfo[idx][dIY], DoorInfo[idx][dIZ]);
	    format(string, sizeof(string), " Ha configurado el ID de la puerta %d en sus coordenadas interiores.. (Int: %d | VW: %d)", idx, GetPlayerInterior(playerid), GetPlayerVirtualWorld(playerid));
	    SendClientMessage(playerid, COLOR_WHITE, string);
	}
	if(!strcmp(params, "exterior", true, 8))
	{
	    if(sscanf(params, "s[128]i", params, idx, text)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /dedit interior [doorid]");
        if(DoorInfo[idx][dType] == 0) return SendClientMessage(playerid, COLOR_GREY, "Invalid door id.");
		DoorInfo[idx][dOInt] = GetPlayerInterior(playerid);
		DoorInfo[idx][dOVW] = GetPlayerVirtualWorld(playerid);
		GetPlayerFacingAngle(playerid, DoorInfo[idx][dOA]);
		GetPlayerPos(playerid, DoorInfo[idx][dOX], DoorInfo[idx][dOY], DoorInfo[idx][dOZ]);
		DestroyDynamicPickup(DoorInfo[idx][dPickup]);
		DoorInfo[idx][dPickup] = CreateDynamicPickup(DoorInfo[idx][dType], 1, DoorInfo[idx][dOX], DoorInfo[idx][dOY], DoorInfo[idx][dOZ], DoorInfo[idx][dOVW], DoorInfo[idx][dOInt]);
		DestroyDynamic3DTextLabel(DoorInfo[idx][dTextID]);
		format(string, sizeof(string), "ID: %d\n%s", idx, DoorInfo[idx][dText]);
		DoorInfo[idx][dTextID] = CreateDynamic3DTextLabel(string, COLOR_WHITE, DoorInfo[idx][dOX], DoorInfo[idx][dOY], DoorInfo[idx][dOZ]+0.3, 15);
	    format(string, sizeof(string), " Has configurado el ID de puerta  %d exterior de tus coordenadas.. (Int: %d | VW: %d)", idx, GetPlayerInterior(playerid), GetPlayerVirtualWorld(playerid));
	    SendClientMessage(playerid, COLOR_WHITE, string);
	}
	if(!strcmp(params, "custominterior", true, 14))
	{
	    if(sscanf(params, "s[128]i", params, idx)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /dedit custominterior [doorid]");
        if(DoorInfo[idx][dType] == 0) return SendClientMessage(playerid, COLOR_GREY, "Invalid door id.");
		if(!DoorInfo[idx][dCInt])
		{
			DoorInfo[idx][dCInt] = 1;
		    format(string, sizeof(string), " Ha configurado ID de puerta %d para tener un interior personalizado.", idx, text);
	    }
	    else
	    {
	        DoorInfo[idx][dCInt] = 0;
		    format(string, sizeof(string), " Has establecido ID de puerta %d para tener un interior normal.", idx, text);
	    }
	    SendClientMessage(playerid, COLOR_WHITE, string);
	}
	if(!strcmp(params, "customexterior", true, 14))
	{
	    if(sscanf(params, "s[128]i", params, idx)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /dedit customexterior [doorid]");
        if(DoorInfo[idx][dType] == 0) return SendClientMessage(playerid, COLOR_GREY, "Invalid door id.");
		if(!DoorInfo[idx][dCExt])
		{
			DoorInfo[idx][dCExt] = 1;
		    format(string, sizeof(string), " Ha configurado ID de puerta% d para tener un exterior personalizado.", idx, text);
	    }
	    else
	    {
	        DoorInfo[idx][dCExt] = 0;
		    format(string, sizeof(string), " Ha configurado ID de puerta% d para tener un exterior normal.", idx, text);
	    }
	    SendClientMessage(playerid, COLOR_WHITE, string);
	}
	return 1;
}

