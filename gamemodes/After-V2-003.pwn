CMD:sacardecasa(playerid, params[])
{
	new string[128], option[32], amount;
	new idx = PlayerInfo[playerid][pHouse];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(!PlayerInfo[playerid][pHouse] && !PlayerInfo[playerid][pVHouse]) return SendClientMessage(playerid, COLOR_GREY, "Usted no es dueño de una casa.");
    if(GetPlayerVirtualWorld(playerid)-500 != idx && !PlayerInfo[playerid][pVHouse]) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de tu casa.");
    if(GetPlayerVirtualWorld(playerid)-500 != idx && PlayerInfo[playerid][pVHouse])
	{
		idx = PlayerInfo[playerid][pVHouse];
	    if(GetPlayerVirtualWorld(playerid)-500 != idx) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de tu casa.");
	}
    if(sscanf(params, "s[32]", option))
    {
        SendClientMessage(playerid, COLOR_WHITE, "USO: /sacardecasa opcion [cantidad]");
        SendClientMessage(playerid, COLOR_GREY, "Artículos: 1) dinero | 2) armas | 3) hierba | 4) cocaina");
        format(string, sizeof(string), "Bóveda: $%d | %d Hierba | %d Cocaina", HouseInfo[idx][hMoney], HouseInfo[idx][hWeed], HouseInfo[idx][hcocaine]);
		SendClientMessage(playerid, COLOR_WHITE, string);
		format(string, sizeof(string), "Bóveda: Arma 1: %s (%d Municion) | Arma 2: %s (%d Municion) | Arma 3: %s (%d Municion)", RWN(HouseInfo[idx][hGun][0]), HouseInfo[idx][hGunAmmo][0], RWN(HouseInfo[idx][hGun][1]), HouseInfo[idx][hGunAmmo][1], RWN(HouseInfo[idx][hGun][2]), HouseInfo[idx][hGunAmmo][2]);
        SendClientMessage(playerid, COLOR_WHITE, string);
		format(string, sizeof(string), "Bóveda: Arma 4: %s (%d Municion) | Arma 5: %s (%d Municion)", RWN(HouseInfo[idx][hGun][3]), HouseInfo[idx][hGunAmmo][3], RWN(HouseInfo[idx][hGun][4]), HouseInfo[idx][hGunAmmo][4]);
        SendClientMessage(playerid, COLOR_WHITE, string);
		return 1;
    }
    if(!strcmp(option, "dinero", true, 5))
    {
    	if(sscanf(params, "s[32]i", option, amount)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /sacardecasa dinero [cantidad]");
     	if(HouseInfo[idx][hMoney] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes el dinero suficiente en la bóveda de tu casa.");
    	HouseInfo[idx][hMoney] -= amount;
     	GiveDodMoney(playerid, amount);
     	format(string, sizeof(string), "* %s abre su bóveda, saca $%d y lo cierra.", RPN(playerid), amount);
