CMD:ad(playerid, params[])
{
	new idx = GetPlayerVirtualWorld(playerid)-100, price = strlen(params) * 6, string[128];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pLevel] < 2) SendClientMessage(playerid, COLOR_GREY, "Necesitar sel nivel 2 para crear un anuncio.");
	if(PlayerInfo[playerid][pADMute] > 0) return SendClientMessage(playerid, COLOR_GREY, "Estas muteado temporalmente de hacer anuncios.");
	if(idx > 0 && idx < MAX_BIZ && BizInfo[idx][bType] != 5 || idx < 1 || idx > MAX_BIZ) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de una agencia de publicidad..");
	if(sscanf(params, "s[128]", params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /ad [advertisement]");
	if(strlen(params) > 100) return SendClientMessage(playerid, COLOR_GREY, "El anuncio no puede tener mas de 100 caracteres.");
	if(PlayerInfo[playerid][pMoney] < price) return SendClientMessage(playerid, COLOR_GREY, "No tienes el dinero suficiente para hacer esto.");
	if(HasAD[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Debes esperar a que se publique tu anuncio para poner otro.");
	if(AdQueue == MAX_ADS-1) return SendClientMessage(playerid, COLOR_GREY, " La cola de anuncios está llena");
	// Advertisement
	HasAD[playerid] = 1;
	if(LastAd) SendClientMessage(playerid, COLOR_LIGHTBLUE, " Su anuncio se ha colocado en la cola, espere unos 60 segundos como máximo.");
    format(AD[AdQueue], 128, "Anuncio: %s, - Tlf: %d", params, PlayerInfo[playerid][pNumber]);
    iAD[AdQueue] = 0;
    format(iAdName[AdQueue], 32, "%s", BizInfo[idx][bOwner]);
    AdQueue++;
    GiveDodMoney(playerid, -price);
    BizInfo[idx][bMoney] += price/2;
    BizInfo[idx][bProducts] --;
    NewsMoney = price/2;
    format(string, sizeof(string), "* %s Ha puesto un anuncio en la cola por $%d.", RPN(playerid), price);
	SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	SendClientMessage(playerid, COLOR_WHITE, " Se te ha cobrado $5 por caracteres.");
	return 1;
}

