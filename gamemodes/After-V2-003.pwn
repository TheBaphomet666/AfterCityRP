	if(seatid < 2 || seatid > 3) return SendClientMessage(playerid, COLOR_GREY, "Solo puedes ponerlo en asiento 2 y 3.");
	if(IsPlayerInAnyVehicle(playerb)) return SendClientMessage(playerid, COLOR_GREY, "El jugador está en un vehiculo.");
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Debes estar afuera de tu vehiculo!");
 	new carid = LastCar[playerid];
    new Float:pos[6];
	if(!IsPlayerInAnyVehicle(playerid))
    {
  		PutPlayerInVehicle(playerb, carid, seatid);
  		GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
    	GetPlayerPos(playerb, pos[3], pos[4], pos[5]);
     	GetVehiclePos(carid, pos[0], pos[1], pos[2]);
      	if (floatcmp(floatabs(floatsub(pos[0], pos[3])), 10.0) != -1 &&
       	floatcmp(floatabs(floatsub(pos[1], pos[4])), 10.0) != -1 &&
        floatcmp(floatabs(floatsub(pos[2], pos[5])), 10.0) != -1) return false;
        format(string, sizeof(string), "* fuiste montado por %s.", RPN(playerid));
        SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
        format(string, sizeof(string), "* montaste al sospechoso %s.", RPN(playerb));
        SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
        PutPlayerInVehicle(playerb,carid,seatid);
        TogglePlayerControllable(playerb, false);
        format(string, sizeof(string), "* %s toma a %s de sus esposas y lo mete al vehiculo.", RPN(playerid), RPN(playerb));
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	}
	return 1;
}

