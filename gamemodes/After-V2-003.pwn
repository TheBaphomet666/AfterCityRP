	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " %s ha invitado a la familia  {33AA33}%s {33CCFF}. (/aceptar familia) (/cancelar familia)", RPN(playerid), FamInfo[idx][fName]);
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	return 1;
}

CMD:fdescontratar(playerid, params[])
{
	new playerb, string[128], idx = PlayerInfo[playerid][pFam];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pFamRank] < 5) return SendClientMessage(playerid, COLOR_GREY, "No eres un líder de familia.");
	if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /fdescontratar [playerid]");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(PlayerInfo[playerb][pFam] != PlayerInfo[playerid][pFam]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no está en tu familia.");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "You can't unnvite yourself.");
	if(!strcmp(RPN(playerb), FamInfo[idx][fLeader], false)) return SendClientMessage(playerid, COLOR_GREY, "No puedes desentender al líder de la familia.");
	PlayerInfo[playerb][pFam] = 0;
	PlayerInfo[playerb][pFamRank] = 0;
	FamInfo[idx][fMembers] --;
	format(string, sizeof(string), " Has expulsado a %s de la familia {33AA33}%s {33CCFF}.", RPN(playerb), FamInfo[idx][fName]);
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " %s te ha expulsado de la familia {33AA33}%s {33CCFF}.", RPN(playerid), FamInfo[idx][fName]);
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);

	return 1;
}

