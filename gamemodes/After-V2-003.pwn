CMD:ruleta(playerid, params[])
{
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!IsPlayerInRangeOfPoint(playerid, 90, 1957.48,1017.89,992.47) && !IsPlayerInRangeOfPoint(playerid, 90, 2235.74,1616.43,1006.18) && !IsPlayerInRangeOfPoint(playerid, 90, 1132.9700,-15.2202,1000.6797)) return SendClientMessage(playerid, COLOR_GREY, "No estás en un Casino.");
	new money,bet;
	if(sscanf(params,"ii",bet,money)) return SendClientMessage(playerid,-1,"Utiliza /ruleta [numero] [cantidad]");
	if (money<100) return SendClientMessage(playerid,-1,"La apuesta minima es de $100");
