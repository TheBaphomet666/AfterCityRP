	if(sscanf(params, "uii", playerb, slot, idx)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /makegateowner [playerid] [slot(1-3)] [gateid] {AFAFAF}(Gate ID 0 to remove ownership)");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(idx > 0 && !GateInfo[idx][gModel] || idx < 0) return SendClientMessage(playerid, COLOR_GREY, "Invalid gate id.");
	if(slot < 1 || slot > 3) return SendClientMessage(playerid, COLOR_GREY, "Gate slots are between 1 and 3.");
	switch(slot)
	{
	    case 1: PlayerInfo[playerb][pGate][0] = idx;
	    case 2: PlayerInfo[playerb][pGate][1] = idx;
	    case 3: PlayerInfo[playerb][pGate][2] = idx;
	}
	if(idx == 0)
	{
		format(string, sizeof(string), " Administracion: %s removed your gate ownership in slot %d.", RPN(playerid), slot);
		SendClientMessage(playerb, COLOR_WHITE, string);
		format(string, sizeof(string), " Administración: %s has removed %s's gate ownership in slot %d.", RPN(playerid), RPN(playerb), slot);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/gates.log", string);
	}
	else
	{
		format(string, sizeof(string), " Administración: %s has made you an owner of gate ID %d.", RPN(playerid), idx, slot);
		SendClientMessage(playerb, COLOR_WHITE, string);
		format(string, sizeof(string), " Administración: %s has made %s an owner of gate ID %d. (Slot %d)", RPN(playerid), RPN(playerb), idx, slot);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/gates.log", string);
	}
	return 1;
}

