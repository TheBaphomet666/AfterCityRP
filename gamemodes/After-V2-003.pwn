CMD:ccgate(playerid, params[])
{
	new string[128], slot, password[256], idx;
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "is[256]", slot, password)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /ccgate [ranura] [contraseña]");
	if(slot < 1 || slot > 3) return SendClientMessage(playerid, COLOR_GREY, "Ranuras de portones entre 1 y 3.");
	switch(slot)
	{
		case 1: idx = PlayerInfo[playerid][pGate][0];
		case 2: idx = PlayerInfo[playerid][pGate][1];
		case 3: idx = PlayerInfo[playerid][pGate][2];
	}
	if(!idx) return SendClientMessage(playerid, COLOR_GREY, "No tiene una puerta registrada en esta ranura.");
	format(GateInfo[idx][gPassword], 256, "%s", password);
 	format(string, sizeof(string), " Ha cambiado la contraseña de puertas %d (ID:%d) a {FF6347}%s", slot, idx, password);
	SendClientMessage(playerid, COLOR_WHITE, string);
	return 1;
}

