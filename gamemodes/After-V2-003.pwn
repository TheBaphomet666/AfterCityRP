CMD:cropa(playerid, params[])
{
	new idx = GetPlayerVirtualWorld(playerid)-100, string[128], skin;
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(idx > 0 && idx < MAX_BIZ && BizInfo[idx][bType] != 2 || idx < 1 || idx > MAX_BIZ) return SendClientMessage(playerid, COLOR_GREY, "You are not inside a Clothes Shop.");
	if(sscanf(params, "i", skin)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /cropa [skinid]");
	if(BizInfo[idx][bProducts] < 1) return SendClientMessage(playerid, COLOR_GREY, "Este negocio no tiene productos.");
	if(PlayerInfo[playerid][pMoney] < 100) return SendClientMessage(playerid, COLOR_GREY, "No tienes el dinero suficiente para hacer esto.");
	if(skin < 0 || skin > 299 || skin == 0 || skin == 274 || skin == 275 || skin == 276 || skin == 277  || skin == 278 || skin == 279 || skin == 280 || skin == 281 || skin == 282 || skin == 283
	|| skin == 284 || skin == 285 || skin == 286 || skin == 287 || skin == 288) return SendClientMessage(playerid, COLOR_GREY, "Skin invalida.");
	for(new f=1; f<MAX_FAMILIES; f++)
	{
	    if(FamInfo[f][fTaken])
	    {
			for(new s=0; s<7; s++)
			{
			    if(FamInfo[f][fSkins][s] == skin && PlayerInfo[playerid][pFam] != f) return SendClientMessage(playerid, COLOR_GREY, "Esta skin está restringida a una familia.");
			}
	    }
	}
	PlayerInfo[playerid][pModel] = skin;
	SetPlayerSkin(playerid, skin);
	BizInfo[idx][bMoney] += 100;
	BizInfo[idx][bProducts] --;
