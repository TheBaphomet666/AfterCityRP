	}
    else if(!strcmp(params, "mp5", true, 3))
    {
		if(strval(RPJL(playerid, JOB_WDEALER)) < 2) return SendClientMessage(playerid, COLOR_GREY, "Su nivel de distribuidor de armas es demasiado bajo para vender esta arma.");
