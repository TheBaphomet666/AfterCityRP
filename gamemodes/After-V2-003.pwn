CMD:venderautoa(playerid, params[]){
	new playerb, carid,price, string[128];
    if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(!PlayerInfo[playerid][pVeh] && !PlayerInfo[playerid][pVVeh]) return SendClientMessage(playerid, COLOR_GREY, "No tienes un vehiculo.");
	if(!IsPlayerInAnyVehicle(playerid))
	    return Msg(playerid,COLOR_ERROR,"No estás en un vehículo!");
 	if(sscanf(params, "ii",playerb,price)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /venderauto [ID] [precio]");
 	if(!IsPlayerLoggedIn(playerb) || playerid==playerb) return SendClientMessage(playerid, COLOR_GREY, "ID de usuario invalido.");
 	if(PlayerInfo[playerb][pVeh] && PlayerInfo[playerb][pVVeh]) return SendClientMessage(playerid, COLOR_GREY, "El sujeto ya tiene Dos autos.");
 	if(price<=0) return SendClientMessage(playerid, COLOR_GREY, "El precio debe ser mayor a 0.");
 	//if(!IsPlayerNearPlayer(playerid, playerb, 15)) return SendClientMessage(playerid, COLOR_GREY, "Estás muy lejos de ese jugador.");
 	if(PlayerInfo[playerid][pVeh]!=GetPlayerVehicleID(playerid) && PlayerInfo[playerid][pVVeh]!=GetPlayerVehicleID(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Este Auto no es tuyo.");
 	carid=GetPlayerVehicleID(playerid);
	Buyer[playerb] = 1;
	Seller[playerb] = playerid;
	Price[playerb] = price;
	Auto[playerb] = carid;
	format(string, sizeof(string), " Has ofrecido a %s un auto por {33AA33}%d {33CCFF}.", RPN(playerb),price);
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " %s te ha Ofrecido un auto por {33AA33}%d {33CCFF}. (/aceptar auto)", RPN(playerid),price);
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);

	return 1;
}
