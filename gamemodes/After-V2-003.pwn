CMD:reportar(playerid,params[])
{

    new playerb, mensaje[400];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "us[128]", playerb,mensaje)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /reportar [id] [texto]");
	if(PlayerInfo[playerid][pRMute] > 0) return SendClientMessage(playerid, COLOR_GREY, "Estas muteado por reportar.");
	if(AntiAdv(playerid, mensaje)) return 1;
	if(strlen(mensaje) > 128) return SendClientMessage(playerid, COLOR_GREY, "Maximo de caracteres 128.");
	if(ReportTime[playerid] > 0)
	{
	    format(mensaje, sizeof(mensaje), "Necesitas esperar %d segundos antes de poder reportar nuevamente.", ReportTime[playerid]);
		SendClientMessage(playerid, COLOR_GREY, mensaje);
		return 1;
	}
 	format(mensaje, sizeof(mensaje), "Reporte de {FF6347}[%d] {FFFF91}%s a {FF6347}[%d]{FFFF91}%s: {FFFFFF}%s", playerid, RPN(playerid),playerb,RPN(playerb), mensaje);
	SendAdminMessage(COLOR_REPORT, 1, mensaje);
	format(mensaje, sizeof(mensaje), "Reporte de [%d] %s: %s", playerid, RPN(playerid), mensaje);
	Log("logs/reports.log", mensaje);
	SendClientMessage(playerid, COLOR_ORANGE, "Tu reporte se envio a los administradores online, por favor ten paciencia.");
	ReportTime[playerid] = 25;
	SetTimerEx("ReportTimer", 60000, false, "i", playerid);
	ReporteEnviado[playerid] = 1;

	return 1;
}


