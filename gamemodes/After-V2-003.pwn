	// Bank Robbery
	if(BankRobberyTime) BankRobberyTime --;
	    //SendAdminMessage(COLOR_YELLOW, 1, "bank");
	// Bonus Calculation
	new bonus;
	foreach(Player, x)
	{
	    if(IsPlayerLoggedIn(x)) bonus ++;
	}
	bonus = bonus*2;
	    //SendAdminMessage(COLOR_YELLOW, 1, "Bonus");
	// Paycheck
	foreach(Player,i)
	{
	    //new stringge[128];
	    //format(stringge, sizeof(stringge), "Start ID=%d", i);
	    //SendAdminMessage(COLOR_YELLOW, 1, stringge);
	  	    		//SendClientMessage(i, COLOR_FADE1, "A pagar-2");
