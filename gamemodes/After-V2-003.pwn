	if(!PlayerInfo[playerb][pFac]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no esta en la faccion.");
	SetPlayerColor(playerb, TRANSPARENT_WHITE);
	format(string, sizeof(string), " has sido expulsado %s afuera de la faccion {33AA33}%s {33CCFF}", RPN(playerb), RPFNEx(playerb));
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " Administrador %s ha limpiado la faccion {33AA33}%s {33CCFF} Eres un civil.", RPN(playerid), RPFNEx(playerb));
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	FacInfo[idx][facMembers]--;
	if(PlayerInfo[playerb][pFacLeader])
	{
		format(FacInfo[idx][facLeader], MAX_PLAYER_NAME, "None");
	}
	PlayerInfo[playerb][pFac] = 0;
	PlayerInfo[playerb][pFacRank] = 0;
	PlayerInfo[playerb][pFacLeader] = 0;
	PlayerInfo[playerb][pFacDuty] = 0;
	return 1;
}


CMD:fcontratar(playerid, params[])
{
	new playerb, string[128], idx = PlayerInfo[playerid][pFam];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pFamRank] < 5) return SendClientMessage(playerid, COLOR_GREY, "No eres un líder de familia.");
	if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /fcontratar [playerid]");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(PlayerInfo[playerb][pFam]) return SendClientMessage(playerid, COLOR_GREY, "El jugador ya está en una familia.");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes invitarte.");
	// Inviting
	fInvited[playerb] = 1;
	fInviter[playerb] = playerid;
