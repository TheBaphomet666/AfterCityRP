CMD:fps(playerid, params[])
{
	if(IsPlayerInAnyVehicle(playerid))
	{
		if(GetPVarInt(playerid,"FPS_On") == 0)
		{
			new VehicleID = GetPlayerVehicleID(playerid);
			FPSObject[playerid] = CreatePlayerObject(playerid, 19300, 0.0000, -1282.9984, 10.1493, 0.0000, -1, -1, 100);
			AttachPlayerObjectToVehicle(playerid, FPSObject[playerid], VehicleID, -0.314999, -0.195000, 0.510000, 0.000000, 0.000000, 0.000000);
			AttachCameraToPlayerObject(playerid,FPSObject[playerid]);
			SetPVarInt(playerid,"FPS_On",1);
		}
		else
		{
			SetCameraBehindPlayer(playerid);
			DestroyPlayerObject(playerid,FPSObject[playerid]);
			SetPVarInt(playerid,"FPS_On",0);
		}
	}
	else return SendClientMessage(playerid, WHITE, "Debes de estar dentro de un vehículo para utilizar éste comando.");
	return 1;
}

