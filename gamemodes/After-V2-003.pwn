CMD:meterencasa(playerid, params[])
{
	new string[128], option[32], amount, done;
	new idx = PlayerInfo[playerid][pHouse];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(!PlayerInfo[playerid][pHouse] && !PlayerInfo[playerid][pVHouse]) return SendClientMessage(playerid, COLOR_GREY, "Usted no es dueño de una casa.");
    if(GetPlayerVirtualWorld(playerid)-500 != idx && !PlayerInfo[playerid][pVHouse]) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de tu casa.");
    if(GetPlayerVirtualWorld(playerid)-500 != idx && PlayerInfo[playerid][pVHouse])
	{
		idx = PlayerInfo[playerid][pVHouse];
	    if(GetPlayerVirtualWorld(playerid)-500 != idx) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de tu casa.");
	}
	if(sscanf(params, "s[32]", option))
    {
        SendClientMessage(playerid, COLOR_WHITE, "USO: /meterencasa opcion [artículo]");
        SendClientMessage(playerid, COLOR_GREY, "Artículos: dinero | arma | hierba | cocaina");
        format(string, sizeof(string), "Bóveda: $%d | %d Hierba | %d Cocaina", HouseInfo[idx][hMoney], HouseInfo[idx][hWeed], HouseInfo[idx][hcocaine]);
		SendClientMessage(playerid, COLOR_WHITE, string);
		format(string, sizeof(string), "Bóveda: Arma 1: %s (%d Municion) | Arma 2: %s (%d Municion) | Arma 3: %s (%d Municion)", RWN(HouseInfo[idx][hGun][0]), HouseInfo[idx][hGunAmmo][0], RWN(HouseInfo[idx][hGun][1]), HouseInfo[idx][hGunAmmo][1], RWN(HouseInfo[idx][hGun][2]), HouseInfo[idx][hGunAmmo][2]);
        SendClientMessage(playerid, COLOR_WHITE, string);
		format(string, sizeof(string), "Bóveda: Arma 4: %s (%d Municion) | Arma 5: %s (%d Municion)", RWN(HouseInfo[idx][hGun][3]), HouseInfo[idx][hGunAmmo][3], RWN(HouseInfo[idx][hGun][4]), HouseInfo[idx][hGunAmmo][4]);
        SendClientMessage(playerid, COLOR_WHITE, string);
		return 1;
    }
	if(sscanf(params, "s[32]", option))
	{
		SendClientMessage(playerid, COLOR_WHITE, "USO: /meterencasa opcion [artículo]");
		SendClientMessage(playerid, COLOR_GREY, "Artículo: 1) dinero | 2) arma | 3) hierba | 4) cocaina");
		return 1;
	}
	if(!strcmp(option, "dinero", true, 5))
	{
		if(sscanf(params, "s[32]i", option, amount)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /meterencasa dinero [cantidad]");
		if(PlayerInfo[playerid][pMoney] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes el dinero suficiente.");
		HouseInfo[idx][hMoney] += amount;
		GiveDodMoney(playerid, -amount);
		format(string, sizeof(string), "* %s abre su bóveda, pone $%d adentro y lo cierra.", RPN(playerid), amount);
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	}
	else if(!strcmp(option, "arma", true, 4))
	{
		if(!GetPlayerWeapon(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No estás sosteniendo un arma en tu mano.");
		new tw[2], w[2];
		for(new i=0; i<13; i++)
		{
 			GetPlayerWeaponData(playerid, i, tw[0], tw[1]);
			if(tw[0] == GetPlayerWeapon(playerid))
			{
				w[0] = tw[0];
				w[1] = tw[1];
			}
		}
		if(!HouseInfo[idx][hGun][0])
		{
			HouseInfo[idx][hGun][0] = w[0];
			HouseInfo[idx][hGunAmmo][0] = w[1];
			done = 1;
		}
		else if(!HouseInfo[idx][hGun][1])
		{
			HouseInfo[idx][hGun][1] = w[0];
			HouseInfo[idx][hGunAmmo][1] = w[1];
			done = 1;
		}
		else if(!HouseInfo[idx][hGun][2])
		{
			HouseInfo[idx][hGun][2] = w[0];
			HouseInfo[idx][hGunAmmo][2] = w[1];
			done = 1;
		}
		else if(!HouseInfo[idx][hGun][3])
		{
			HouseInfo[idx][hGun][3] = w[0];
			HouseInfo[idx][hGunAmmo][3] = w[1];
			done = 1;
		}
		else if(!HouseInfo[idx][hGun][4])
		{
			HouseInfo[idx][hGun][4] = w[0];
			HouseInfo[idx][hGunAmmo][4] = w[1];
			done = 1;
		}
		if(done)
		{
			format(string, sizeof(string), "* %s abre su bóveda, pone un %s adentro y lo cierra.", RPN(playerid), RWN(GetPlayerWeapon(playerid)));
			SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
			RemoveDodWeapon(playerid, w[0]);
		}
		else SendClientMessage(playerid, COLOR_GREY, "La bóveda de tu casa no puede contener más armas.");
	}
	else if(!strcmp(option, "hierba", true, 4))
	{
		if(sscanf(params, "s[32]i", option, amount)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /meterencasa hierba [cantidad]");
		if(PlayerInfo[playerid][pWeed] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes mucha hierba en ti.");
		HouseInfo[idx][hWeed] += amount;
		PlayerInfo[playerid][pWeed] -= amount;
		format(string, sizeof(string), "* %s abre su bóveda, pone %d gramos de hierba dentro y la cierra.", RPN(playerid), amount);
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	}
	else if(!strcmp(option, "cocaina", true, 4))
	{
		if(sscanf(params, "s[32]i", option, amount)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /meterencasa cocaina [cantidad]");
		if(PlayerInfo[playerid][pcocaine] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes mucha cocaína en ti.");
		HouseInfo[idx][hcocaine] += amount;
		PlayerInfo[playerid][pcocaine] -= amount;
		format(string, sizeof(string), "* %s abre su bóveda, pone %d gramos de cocaína dentro y lo cierra.", RPN(playerid), amount);
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	}
	return 1;
}

