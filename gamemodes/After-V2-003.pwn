CMD:aceptar(playerid, params[])
{
	new string[128];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "s[128]", params))
	{
        SendClientMessage(playerid, COLOR_WHITE, "USO: /aceptar [opcion]");
		SendClientMessage(playerid, COLOR_GREY, "Opciones: faccion | materiales  armas | liberar | luchar  chaleco");
		SendClientMessage(playerid, COLOR_GREY, "Opciones: cocaina | hierba | sexo| defender || entrevista |");
		return 1;
	}
	if(!strcmp(params, "faccion", true))
	{
	    if(!Invited[playerid]) return SendClientMessage(playerid, COLOR_GREY, "No has sido invitado a una facción.");
	    {
	        new idx = Faction[playerid];
			PlayerInfo[playerid][pFac] = Faction[playerid];
			PlayerInfo[playerid][pFacRank] = 0;
			FacInfo[idx][facMembers]++;
			PlayerInfo[playerid][pFacLeader] = 0;
			PlayerInfo[playerid][pFacDiv] = 0;
			PlayerInfo[playerid][pFacDivLeader] = 0;
			Invited[playerid] = 0;
			Faction[playerid] = 0;
			format(string, sizeof(string), " Has aceptado la invitación de %s's para unirte a la facción {33AA33}%s {33CCFF}.", RPN(Inviter[playerid]), RPFNEx(playerid));
			SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
			format(string, sizeof(string), " %s ha aceptado su invitación de facción.", RPN(playerid));
			SendClientMessage(Inviter[playerid], COLOR_LIGHTBLUE, string);
			return 1;
	    }
	}
	else if(!strcmp(params, "familia", true))
	{
	    if(!fInvited[playerid]) return SendClientMessage(playerid, COLOR_GREY, "No has sido invitado a una familia.");
	    {
	        new idx = Family[playerid];
			PlayerInfo[playerid][pFam] = Family[playerid];
			PlayerInfo[playerid][pFamRank] = 1;
			FamInfo[idx][fMembers] ++;
			fInvited[playerid] = 0;
			Family[playerid] = 0;
			format(string, sizeof(string), " Has aceptado la invitación de %s's para unirte a la familia {33AA33}%s {33CCFF}.", RPN(fInviter[playerid]), FamInfo[idx][fName]);
			SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
			format(string, sizeof(string), " %s ha aceptado su invitación familiar.", RPN(playerid));
			SendClientMessage(fInviter[playerid], COLOR_LIGHTBLUE, string);
			return 1;
	    }
	}
	else if(!strcmp(params, "matrimonio", true))
	{
	    if(!Proponido[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Nadie se quiere casar contigo.");
	    {
	        
			format(PlayerInfo[playerid][pPareja], 64, "%s", RPNU(Propone[playerid]));
			format(PlayerInfo[Propone[playerid]][pPareja], 64, "%s", RPNU(playerid));
			format(string, sizeof(string), " Has aceptado Casarte con {33AA33}%s {33CCFF}.", RPN(Propone[playerid]));
			SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
			format(string, sizeof(string), " %s ha aceptado su Proposición de matrimonio.", RPN(playerid));
			SendClientMessage(Propone[playerid], COLOR_LIGHTBLUE, string);
			Proponido[playerid] = 0;
			Propone[playerid] = 0;
			return 1;
	    }
	}
 	else if(!strcmp(params, "auto", true))
	{
	    if(Buyer[playerid])
	    {
	        if(PlayerInfo[playerid][pMoney] < Price[playerid]){return SendClientMessage(playerid, COLOR_GREY, "No tienes mucho dinero en ti ahora mismo.");}

			if(PlayerInfo[playerid][pVeh]==0)
			{
                PlayerInfo[playerid][pVeh]=Auto[playerid];
                PlayerInfo[playerid][vModel]=GetVehicleModel(Auto[playerid]);
                if(PlayerInfo[Seller[playerid]][pVeh]==Auto[playerid]){
                	PlayerInfo[Seller[playerid]][pVeh]=0;
                	PlayerInfo[Seller[playerid]][vModel]=0;
                	
                }
                else if(PlayerInfo[Seller[playerid]][pVVeh]==Auto[playerid]){
                    PlayerInfo[Seller[playerid]][pVVeh]=0;
                    PlayerInfo[Seller[playerid]][vVModel]=0;
                }
                GiveDodMoney(playerid,-Price[playerid]);
                GiveDodMoney(Seller[playerid],Price[playerid]);
                for(new i=1; i<PCAR_LIMIT; i++)
	            {
	                new file[64];
					format(file,sizeof file,"/pcar/%i.txt",i);
					new id;
					id=dini_Int(file,"ID");
					if(Auto[playerid]==id)
					{
					    new name[30];
						GetPlayerName(playerid, name, sizeof(name));
						dini_Set(file,"Owner",name);
					    break;
					}
	            }
				format(string, sizeof(string), " Has aceptado comprar el auto de %s por un valor de{33AA33}%d {33CCFF}.", RPN(Seller[playerid]), Price[playerid]);
				SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
