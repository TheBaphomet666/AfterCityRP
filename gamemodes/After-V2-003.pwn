	if(BankRobber[playerid] != 1) return SendClientMessage(playerid, COLOR_GREY, "Usted no es el líder de robo de banco.");
	if(RobberyStarted) return SendClientMessage(playerid, COLOR_GREY, "El robo del banco ya ha comenzado.");
	if(BankRobbersCount < 2) return SendClientMessage(playerid, COLOR_GREY, "No hay suficientes ladrones para robar al banco.");
	if(!IsPlayerInRangeOfPoint(playerid, 5, 2144.14, 1627.14, 994.350)) return SendClientMessage(playerid, COLOR_GREY, "No estás cerca de la bóveda del banco.");
	BankC4 = CreateDynamicObject(1654, 2144.14, 1627.14, 993.350, 0, 89.325012207031, 3.9700012207031);
	RobberyStarted = 1;
	SetTimer("BankVaultTimer", 10000, false);
	SendRobberyMessage(COLOR_LIGHTRED, "** La bóveda del banco va a explotar en 10 segundos, alejarse de ella **");
	SendCopMessage(COLOR_BLUE, "LS Banco: Hay un grupo de personas que intentan robar al banco!");
	SendFBIMessage(COLOR_DBLUE, "LS Banco: Hay un grupo de personas que intentan robar al banco!");
	foreach(Player, i)
	{
	    if(PlayerInfo[i][pFac] == 1 || PlayerInfo[i][pFac] == 6 )
	    {
	        TextDrawShowForPlayer(i, Textdraw0);
			TextDrawShowForPlayer(i, Textdraw1);
	    	if(BankRobbersCount >= 2) {TextDrawShowForPlayer(i, Textdraw2);}
			else if(BankRobbersCount >= 3) {TextDrawShowForPlayer(i, Textdraw3);}
			else if(BankRobbersCount >= 4) {TextDrawShowForPlayer(i, Textdraw4);}
			else if(BankRobbersCount >= 5) {TextDrawShowForPlayer(i, Textdraw5);}
	    }
	    if(BankRobber[i])
	    {
		    ClearDodWantedLevels(i);
		    AddDodWantedLevel(i, "Robo a banco");
		    AddDodWantedLevel(i, "Robo a banco");
		    AddDodWantedLevel(i, "Robo a banco");
		    AddDodWantedLevel(i, "Robo a banco");
		    AddDodWantedLevel(i, "Robo a banco");
		    AddDodWantedLevel(i, "Robo a banco");
		    TotalRobbers ++;
	    }
	}
	BankRobberyTime = 4;
	ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0);
	ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0);
	foreach(Player, i)
	{
	    if(!BankRobber[i] && GetPlayerInterior(i) == 83 && GetPlayerVirtualWorld(i) == 8473)
	    {
	        SetPlayerPos(i, 1462.4095,-1011.1602,26.8438);
	        SetPlayerInterior(i, 0);
	        SetPlayerVirtualWorld(i, 0);
	        SendClientMessage(i, COLOR_LIGHTRED, "** El banco está siendo robado, usted ha sido expulsado por los ladrones. **");
	    }
	}
	BankBlock = 1;
	return 1;
}



