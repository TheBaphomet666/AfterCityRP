CMD:arrestar(playerid, params[])
{
	new playerb, time, string[128];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!IsACop(playerid) && !IsADOC(playerid) && !IsASAAF(playerid) && !IsFBI(playerid) && !IsNG(playerid) && !IsSASD(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No eres un oficial de la ley.");
	if(!PlayerInfo[playerid][pFacDuty]) return SendClientMessage(playerid, COLOR_GREY, "No estás en Servicio.");
	if(!IsPlayerInRangeOfPoint(playerid, 2, ap[0], ap[1], ap[2]) && !IsPlayerInRangeOfPoint(playerid, 2, ap2[0], ap2[1], ap2[2])) return SendClientMessage(playerid, COLOR_GREY, "Usted no está cerca del punto de detención.");
	if(sscanf(params, "uis[64]", playerb, time, params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /arrestarar [playerid] [minutos] [razón]");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(!PlayerInfo[playerb][pWanted]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no es querido.");
	if(IsACop(playerb) || IsFBI(playerb) || IsNG(playerb) || IsSASD(playerb) || IsAGov(playerb)) return SendClientMessage(playerid, COLOR_GREY, "No puedes arrestar a un oficial de LSPD.");
	if(!IsPlayerNearPlayer(playerid, playerb, 2)) return SendClientMessage(playerid, COLOR_GREY, "Estás muy lejos de ese jugador.");
	format(string, sizeof(string), "Prisión local: %s ha sido arrestada por %s Razón: %s.", RPN(playerb), RPN(playerid), params);
	foreach(Player, i)
	{
	    if(IsACop(i) || IsFBI(i) || IsNG(i) || IsSASD(i))
	    {
	        SendClientMessage(i, COLOR_BLUE, string);
	    }
	}
	PlayerInfo[playerb][pArrested] ++;
	PlayerInfo[playerb][pPrison] = 1;
	PlayerInfo[playerb][pPrisonTime] = time*60;
	SetPlayerColor(playerb, TRANSPARENT_ORANGE);
	ClearDodWantedLevels(playerb);
	SetPlayerInterior(playerb, 0);
	SetPlayerVirtualWorld(playerb, 0);
	new RandomCell = random(sizeof(RandomPrison));
	SetPlayerFacingAngle(playerb, RandomPrison[RandomCell][3]);
	TogglePlayerControllable(playerb, 0);
	SetTimerEx("EnterExitTimer", 5000, false, "i", playerb);
 	SetPlayerPos(playerb, 1810.50,-1548.37,5700.43);
 	SetCameraBehindPlayer(playerb);
	format(string, sizeof(string), " Usted ha sido arrestado por un oficial durante %d minutos (%d segundos)", PlayerInfo[playerb][pPrisonTime]/60, PlayerInfo[playerb][pPrisonTime]);
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	RemovePlayerAttachedObject(playerb, 0);
	SetPlayerSpecialAction(playerb, SPECIAL_ACTION_NONE);
	if(g_bIsRobbiz[playerb])
	    {
	        DisablePlayerCheckpoint(playerb);
	        DestroyObject(g_robbizBag[playerb]);
	        g_apprehendedCash[playerb] = 0;
			g_bIsRobbiz[playerb] = false;
			new copMessage[64];
			format(copMessage, sizeof(copMessage), "** %s ha sido Detenido y ha fallado su robo a negocio.", RPN(playerb));
			SendCopMessage(COLOR_BLUE, copMessage);
		}
	// Clearing robbery
 	if(BankRobber[playerb])
    {
	    new Rob = BankRobber[playerb]-1;
	    DisablePlayerCheckpoint(playerb);
		BankRobbersCount --;
		format(BankRobbers[Rob], MAX_PLAYER_NAME, "");
		DeliverMoney[playerb] = 0;
		DestroyObject(g_robbizBag[playerb]);
		g_bIsRobbiz[playerb] = false;
		//
		foreach(Player, i)
		{
		    if(BankRobber[i] || PlayerInfo[i][pFac] == 1)
		    {
				if(Rob == 0) TextDrawHideForPlayer(i, Textdraw1);
				else if(Rob == 1) TextDrawHideForPlayer(i, Textdraw2);
				else if(Rob == 2) TextDrawHideForPlayer(i, Textdraw3);
				else if(Rob == 3) TextDrawHideForPlayer(i, Textdraw4);
				else if(Rob == 4) TextDrawHideForPlayer(i, Textdraw5);
			}
		}
		format(string, sizeof(string), "** %s ha sido detenido y ha fallado el robo. **", RPN(playerb));
		SendRobberyMessage(COLOR_LIGHTRED, string);
		SendCopMessage(COLOR_LIGHTRED, string);
		BankRobber[playerb] = 0;
		if(BankRobbersCount == 0)
		{
			TextDrawHideForAll(Textdraw0);
			TextDrawHideForAll(Textdraw1);
			TextDrawHideForAll(Textdraw2);
			TextDrawHideForAll(Textdraw3);
			TextDrawHideForAll(Textdraw4);
			TextDrawHideForAll(Textdraw5);
            BankRobbery = 0;
			RobberyStarted = 0;
			TotalRobbers = 0;
            new copsonline;
			foreach(Player, i)
			{
			    if(IsACop(i) || IsFBI(i) || IsNG(i) || IsSASD(i)) copsonline++;
			}
			foreach(Player, i)
			{
			    if(IsACop(i) || IsFBI(i) || IsNG(i) || IsSASD(i))
			    {
					new Saved = (TotalRobbers*50000-(TotalStolen*50000));
					format(string, sizeof(string), "** Usted ha recibido su corte del dinero ahorrado. ($%d)", Saved/copsonline);
					SendClientMessage(i, COLOR_LIME, string);
			    }
			}
  }
    }
	return 1;
}

