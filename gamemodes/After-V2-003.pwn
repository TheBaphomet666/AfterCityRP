CMD:s(playerid, params[])
{
    new playerb, text[128], string[128];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "us[128]", playerb, text)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /s [ID] [texto]");
	if(AntiAdv(playerid, params)) return 1;
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "ID de usuario invalido.");
	if(!strlen(text)) return SendClientMessage(playerid, COLOR_GREY, "No has ingresado texto.");
	if(IsPlayerNearPlayer(playerid, playerb, 1))
	{

		if(strlen(PlayerInfo[playerid][pAccent])) format(string, sizeof(string), "Susurro a %s [Acento %s]: %s", RPN(playerb), PlayerInfo[playerid][pAccent], text);
		else format(string, sizeof(string), "Susurro a %s: %s", RPN(playerb), text);
		SendClientMessage(playerid, COLOR_YELLOW, string);
		if(strlen(PlayerInfo[playerid][pAccent])) format(string, sizeof(string), "Susurro de %s [Acento %s]: %s", RPN(playerid), PlayerInfo[playerid][pAccent], text);
		else format(string, sizeof(string), "Susurro de %s: %s", RPN(playerid), text);
		SendClientMessage(playerb, COLOR_YELLOW, string);
		new log[256];
		format(log, sizeof(log), "%s a %s: %s", RPN(playerid), RPN(playerb), text);
		Log("logs/susurros.log", log);
		foreach(Player, i)
		{
		    if(PlayerInfo[i][pAdmin] >= 6 && Whispers[i] && i != playerid && i != playerb)
		    {
		        format(log, sizeof(log), "[SUSURRO] %s a %s: %s", RPN(playerid), RPN(playerb), text);
		        SendClientMessage(i, COLOR_YELLOW, log);
		    }
		}
	}
	else
	{
	    SendClientMessage(playerid, COLOR_GREY, "No estas cerca del usuario.");
	}
	return 1;
}

