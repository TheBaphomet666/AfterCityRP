CMD:dar(playerid, params[])
{
	new string[128], playerb, amount = PlayerInfo[playerid][pWeed] && PlayerInfo[playerid][pcocaine] && PlayerInfo[playerid][pMaterials];
	if(sscanf(params, "us[32]d", playerb, params, amount))
	{
		SendClientMessage(playerid, COLOR_WHITE, "USO: /dar [playerid] [opcion] [cantidad]");
		SendClientMessage(playerid, COLOR_GREY, "Opciones: hierba | cocaina | Materiales | arma");
		return 1;
	}
	if(!IsPlayerNearPlayer(playerid, playerb, 5)) return SendClientMessage(playerid, COLOR_GREY, "No puedes dar nada a alguien desde esta distancia.");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(IsPlayerCuffed(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No Puedes hacer eso ahora mismo.");
    if(!strcmp(params, "hierba", true))
	{
	    //if(GetPlayerDrunkLevel(playerid) >= 5000) return SendClientMessage(playerid, COLOR_GREY, "You are too drunk/stoned to use drugs.");
	    if(amount > PlayerInfo[playerid][pWeed] || amount < 1) return SendClientMessage(playerid, COLOR_GREY, "  No tienes mucho.");
	    PlayerInfo[playerid][pWeed] = PlayerInfo[playerid][pWeed] -amount;
	    PlayerInfo[playerb][pWeed] = PlayerInfo[playerb][pWeed] +amount;
	    format(string, sizeof(string), "* %s saca una bolsa de hierba y la entrega a %s.", RPN(playerid), RPN(playerb));
	    SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	    format(string, sizeof(string), " Has dado %s %d de hierba.", RPN(playerb), amount);
	    SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	    format(string, sizeof(string), " %s le ha dado %d de hierba.", RPN(playerid), amount);
	    SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	}
	if(!strcmp(params, "cocaina", true))
	{
	    //if(GetPlayerDrunkLevel(playerid) >= 5000) return SendClientMessage(playerid, COLOR_GREY, "You are too drunk/stoned to use drugs.");
	    if(amount > PlayerInfo[playerid][pcocaine] || amount < 1) return SendClientMessage(playerid, COLOR_GREY, "   No tienes mucho.");
	    PlayerInfo[playerid][pcocaine] = PlayerInfo[playerid][pcocaine] -amount;
	    PlayerInfo[playerb][pcocaine] = PlayerInfo[playerb][pcocaine] +amount;
	    format(string, sizeof(string), "* %s saca una bolsa de cocaína y la entrega a %s.", RPN(playerid), RPN(playerb));
	    SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	    format(string, sizeof(string), " Has dado %s %d de Cocaína.", RPN(playerb), amount);
	    SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	    format(string, sizeof(string), " %s le ha dado %d de Cocaína.", RPN(playerid), amount);
        SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	}
	if(!strcmp(params, "materiales", true))
	{
	    //if(GetPlayerDrunkLevel(playerid) >= 5000) return SendClientMessage(playerid, COLOR_GREY, "You are too drunk/stoned to use drugs.");
	    if(amount > PlayerInfo[playerid][pMaterials] || amount < 1) return SendClientMessage(playerid, COLOR_GREY, "    No tienes mucho.");
	    PlayerInfo[playerid][pMaterials] = PlayerInfo[playerid][pMaterials] -amount;
	    PlayerInfo[playerb][pMaterials] = PlayerInfo[playerb][pMaterials] +amount;
	    format(string, sizeof(string), "* %s saca una bolsa de materiales y la entrega a %s.", RPN(playerid), RPN(playerb));
	    SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	    format(string, sizeof(string), " Has dado %s %d de Materiales.", RPN(playerb), amount);
	    SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	    format(string, sizeof(string), " %s le ha dado %d de Materiales.", RPN(playerid), amount);
        SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	}
	if(!strcmp(params, "arma", true))
	{
		if(playerb == playerid)
		{
		    SendClientMessage(playerid, COLOR_GREY, "No puedes darte tus armas a ti mismo!");
		}
		else if(!IsPlayerConnected(playerb))
		{
		    SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
		}
		else
		{
			new Weapon = GetPlayerWeapon(playerid);
			new Ammos;
			new weapons[13][2];
			for(new i = 0; i < 13; i++)
			{
			    GetPlayerWeaponData(playerid, i, weapons[i][0], weapons[i][1]);
			    if(Weapon == weapons[i][0])
			    {
			        Ammos = weapons[i][1]-amount;
			    }
			}
			if(Ammos<0){
				SendClientMessage(playerid, COLOR_GREY, "No tienes suficientes balas.");
			}
			else{
   				format(string, sizeof(string), " %s le ha dado %d Balas.", RPN(playerid), amount);
        		SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
   				format(string, sizeof(string), "le has dado a %s %d Balas.", RPN(playerb), amount);
        		SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
				GiveDodWeapon(playerb, Weapon, amount);
	            RemoveDodWeapon(playerid, Weapon);
	            if(Ammos>1){
	            	GiveDodWeapon(playerid, Weapon, Ammos);
	            }
            }
		}
	}
	return 1;
}
