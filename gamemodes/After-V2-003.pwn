CMD:darrango(playerid, params[])
{
	new playerb, rank, string[128];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pFacRank]!=10 && PlayerInfo[playerid][pFacRank]!=9) return SendClientMessage(playerid, COLOR_GREY, "No eres un líder de faccion.");
	if(sscanf(params, "ui", playerb, rank)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /darrango [playerid] [rango]");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes cambiar tu rango.");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(PlayerInfo[playerb][pFac] != PlayerInfo[playerid][pFac]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no está en tu facción.");
	if(rank < 1 || rank > 10) return SendClientMessage(playerid, COLOR_GREY, "Los rangos están entre 1 y 10.");
	if(PlayerInfo[playerb][pFacRank] == rank) return SendClientMessage(playerid, COLOR_GREY, "El jugador ya tiene ese rango.");
	if(PlayerInfo[playerb][pFacRank] < rank)
	{
	    PlayerInfo[playerb][pFacRank] = rank;
		format(string, sizeof(string), "Usted ha {33AA33} promovido {33CCFF}%s a {33AA33}%s (%d) {33CCFF}.", RPN(playerb), RPFRN(playerb), rank);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "%s ha {33AA33} promovido {33CCFF} a {33AA33}%s (%d) {33CCFF}.", RPN(playerid), RPFRN(playerb), rank);
		SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
		if(rank == 10)
		{
		    PlayerInfo[playerb][pFacLeader] = 1;
		}
	}
	else if(PlayerInfo[playerb][pFacRank] > rank)
	{
	    PlayerInfo[playerb][pFacRank] = rank;
		format(string, sizeof(string), "Usted {FF9900} desgradó {33CCFF} a %s en {FF9900}%s (%d) {33CCFF}.", RPN(playerb), RPFRN(playerb), rank);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "%s ha {FF9900} desgradado {33CCFF} a {FF9900}%s (%d) {33CCFF}.", RPN(playerid), RPFRN(playerb), rank);
		SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
		if(rank < 10)
		{
		    PlayerInfo[playerb][pFacLeader] = 0;
		}
	}
	return 1;
}

