    SendClientMessage(playerb, COLOR_LIME, string);
	return 1;
}

CMD:liberar(playerid, params[])
{
	new playerb, price, string[128];
    if(FreeTimeout[playerid])
    {
        format(string, sizeof(string), "Tienes que esperar %d más segundos antes de liberar a alguien.", FreeTimeout[playerid]);
        SendClientMessage(playerid, COLOR_GREY, string);
        return 1;
    }
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pJob] != JOB_LAWYER && PlayerInfo[playerid][pVIPJob] != JOB_LAWYER && !IsAGov(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No eres abogado.");
	if(sscanf(params, "ui", playerb, price)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /liberar [playerid] [precio]");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes liberarte.");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(!PlayerInfo[playerb][pPrison]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no está preso.");
	if(!IsPlayerNearPlayer(playerid, playerb, 5)) return SendClientMessage(playerid, COLOR_GREY, "Estás demasiado lejos de tu cliente.");
	if(price < 100 || price > 1000) return SendClientMessage(playerid, COLOR_GREY, "El precio está entre 100 y 2000.");
	if(RPJS(playerid, JOB_LAWYER) < 100) FreeTime[playerb] = 1;
	else if(RPJS(playerid, JOB_LAWYER) < 300) FreeTime[playerb] = 2;
	else if(RPJS(playerid, JOB_LAWYER) < 700) FreeTime[playerb] = 3;
	else if(RPJS(playerid, JOB_LAWYER) < 1200) FreeTime[playerb] = 4;
	else if(RPJS(playerid, JOB_LAWYER) >= 1200) FreeTime[playerb] = 5;
	FreePrice[playerid][playerb] = price;
	FreeBy[playerb] = playerid;
	if(!IsAGov(playerid))
	{
  		format(string, sizeof(string), " Has ofrecido %s para reducir su pena de prisión en %d minutos para $%d.", RPN(playerb), FreeTime[playerb], price);
		SendClientMessage(playerid, COLOR_LIME, string);
