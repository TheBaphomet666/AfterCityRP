CMD:sp(playerid, params[])
{
	new playerb, string[128];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pAdmin] < 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!strcmp(params, "off", true))
	{
	    if(!Spec[playerid]) return SendClientMessage(playerid, COLOR_GREY, "No estas spectando a nadie.");
	    Spec[playerid] = 0;
    	TogglePlayerSpectating(playerid, 0);
    	GetPlayerHealth(playerid, PlayerInfo[playerid][pHealth]);
    	GetPlayerArmour(playerid, PlayerInfo[playerid][pArmor]);
		SetPlayerVirtualWorld(playerid, PlayerInfo[playerid][pVW]);
		SetPlayerInterior(playerid, PlayerInfo[playerid][pInt]);
		SetPlayerPos(playerid, PlayerInfo[playerid][pX], PlayerInfo[playerid][pY], PlayerInfo[playerid][pZ]);
		SendClientMessage(playerid, COLOR_WHITE, " Has dejado de spectar jugadores.");
		if(PlayerInfo[Specid[playerid]][pAdmin] > PlayerInfo[playerid][pAdmin])
   		{
			format(string, sizeof(string), "AdvSpec: %s ha dejado de spectarte.", RPN(playerid));
			SendClientMessage(Specid[playerid], COLOR_DARKRED, string);
	    }
	    Specid[playerid] = -1;
		return 1;
	}
	if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /sp [ID]");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes spectarte a ti mismo.");
    if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "ID de usuario invalido.");
    if(Spec[playerb]) return SendClientMessage(playerid, COLOR_GREY, "El usuario esta spectando a alguien.");
    if(PlayerInfo[playerb][pAdmin] >= 7)return SendClientMessage(playerid, COLOR_RED, "No puedes spectar a los Dioses, ellos te spectean a ti.");
    PlayerInfo[playerid][pModel] = GetPlayerSkin(playerid);
	if(Specid[playerid] != -1 && PlayerInfo[Specid[playerid]][pAdmin] > PlayerInfo[playerid][pAdmin])
	{
			format(string, sizeof(string), "AdvSpec: %s ha dejado de spectarte.", RPN(playerid));
			SendClientMessage(Specid[playerid], COLOR_DARKRED, string);
	}
    if(!Spec[playerid])
    {
	    PlayerInfo[playerid][pVW] = GetPlayerVirtualWorld(playerid);
	    PlayerInfo[playerid][pInt] = GetPlayerInterior(playerid);
	    GetPlayerPos(playerid, PlayerInfo[playerid][pX], PlayerInfo[playerid][pY], PlayerInfo[playerid][pZ]);
    }
    Specid[playerid] = playerb;
    TogglePlayerSpectating(playerid, 1);
    Spec[playerid] = 1;
    SetPlayerInterior(playerid, GetPlayerInterior(playerb));
    SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(playerb));
    format(string, sizeof(string), " Spectando a %s", RPN(playerb));
    SendClientMessage(playerid, COLOR_WHITE, string);
    SendClientMessage(playerid, COLOR_WHITE, "Escribe /sp off para dejar de spectar.");
    if(IsPlayerInAnyVehicle(playerb))
    {
		PlayerSpectateVehicle(playerid, GetPlayerVehicleID(playerb));
    }
    else
    {
        PlayerSpectatePlayer(playerid, playerb);
    }
    if(PlayerInfo[playerb][pAdmin] >= PlayerInfo[playerid][pAdmin])
    {
		format(string, sizeof(string), "AdvSpec: %s te esta spectando.", RPN(playerid));
		SendClientMessage(playerb, COLOR_DARKRED, string);
    }
    if(!PlayerInfo[playerb][pTutorial]) SendClientMessage(playerid, COLOR_WHITE, " El usuario esta viendo el tutorial, no asumas que esta usando cheats aun.");
	return 1;
}

