CMD:revisar(playerid, params[])
{
	new playerb, string[128], weaps, weapons[13][2];
    if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	//if(!IsACop(playerid) && !IsADOC(playerid) && !IsASAAF(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No eres un oficial de la ley.");
	if(sscanf(params,"u",playerb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /recisar [playerid]");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(!IsPlayerNearPlayer(playerid, playerb, 3)) return SendClientMessage(playerid, COLOR_GREY, "No se puede revisar a alguien de esa distancia.");
	if(!IsPlayerCuffed(playerb) && !Tied[playerb] && GetPlayerSpecialAction(playerb) != SPECIAL_ACTION_HANDSUP) return SendClientMessage(playerid, COLOR_GREY, "El Jugador no está esposado, atado ni con las manos levantadas.");
	SendClientMessage(playerid, COLOR_GREEN, "____________________________");
	format(string, sizeof(string), "*** %s Items ***", RPN(playerb));
	SendClientMessage(playerid, COLOR_ORANGE, string);
	format(string, sizeof(string), " Dinero ($%d)", PlayerInfo[playerb][pMoney]);
	SendClientMessage(playerid, COLOR_WHITE, string);
	if(PlayerInfo[playerb][pMaterials]) {format(string, sizeof(string), "Materiales de baja clase (%d)", PlayerInfo[playerb][pMaterials]); SendClientMessage(playerid, COLOR_WHITE, string);}
	if(PlayerInfo[playerb][pMMaterials]) {format(string, sizeof(string), "Materiales de media clase (%d)", PlayerInfo[playerb][pMMaterials]); SendClientMessage(playerid, COLOR_WHITE, string);}
	if(PlayerInfo[playerb][pHMaterials]) {format(string, sizeof(string), "Materiales de alta clase (%d)", PlayerInfo[playerb][pHMaterials]); SendClientMessage(playerid, COLOR_WHITE, string);}
	if(PlayerInfo[playerb][pWeed]) {format(string, sizeof(string), " Hierba (%d)", PlayerInfo[playerb][pWeed]); SendClientMessage(playerid, COLOR_WHITE, string);}
	if(PlayerInfo[playerb][pcocaine]) {format(string, sizeof(string), " cocaina(%d)", PlayerInfo[playerb][pcocaine]); SendClientMessage(playerid, COLOR_WHITE, string);}
	format(string, sizeof(string), "*** %s Armas ***", RPN(playerb));
	SendClientMessage(playerid, COLOR_ORANGE, string);
	for(new i=0; i<13; i++)
	{
       	GetPlayerWeaponData(playerb, i, weapons[i][0], weapons[i][1]);
        if(weapons[i][0] != PlayerInfo[playerb][pWeapon][i] && weapons[i][1] != 0)
        {
	        format(string, sizeof(string), " %s (Municion: %d) (Hackeada)", RWN(weapons[i][0]), weapons[i][1]);
	        SendClientMessage(playerid, COLOR_MEDIC, string);
	        weaps++;
        }
        else if(weapons[i][0] == PlayerInfo[playerb][pWeapon][i] && weapons[i][0] != 0)
        {
            format(string, sizeof(string), " %s (Municion: %d)", RWN(PlayerInfo[playerb][pWeapon][i]), weapons[i][1]);
	        SendClientMessage(playerid, COLOR_MEDIC, string);
	        weaps++;
        }
	}
	if(weaps == 0) return SendClientMessage(playerid, COLOR_MEDIC, "Nada");
	format(string, sizeof(string), "* %s ha revisado %s.", RPN(playerid), RPN(playerb));
	SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	return 1;
}

