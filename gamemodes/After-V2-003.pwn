CMD:bedit(playerid, params[])
{
	new bizid, string[128], input;
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessage(playerid, COLOR_GREY, "You are not an authorized to use this command.");
	if(sscanf(params, "s[32]", params))
	{
		SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit [opcion] [negocioID]");
		SendClientMessage(playerid, COLOR_GREY, "Opciones: posicion | precio | nivel | productos | boveda |puerta");
		return 1;
	}
	if(!strcmp(params, "posicion", true, 8))
	{
	    if(sscanf(params, "s[32]i", params, bizid)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit posicion [bizid]");
		new idx=bizid;
		if(!BizInfo[bizid][bType]) return SendClientMessage(playerid, COLOR_GREY, "Invalid business id.");
		GetPlayerPos(playerid, BizInfo[bizid][bX], BizInfo[bizid][bY], BizInfo[bizid][bZ]);
		DestroyDynamicPickup(BizInfo[bizid][bPickup]);
    	BizInfo[bizid][bPickup] = CreateDynamicPickup(1274, 1, BizInfo[bizid][bX], BizInfo[bizid][bY], BizInfo[bizid][bZ], 0);
		DestroyDynamic3DTextLabel(BizInfo[bizid][bText]);
        if(!strcmp("El Estado", BizInfo[idx][bOwner])) format(string, sizeof(string), "Propiedad: %s\nNegocio Tipo: %s\nEstado: En Venta\nPrecio: $%d", BizInfo[idx][bOwner], RBT(idx), BizInfo[idx][bPrice]);
		else format(string, sizeof(string), "Negocio de %s\nNegocio Tipo: %s\n%s", BizInfo[idx][bOwner], RBT(idx), RBS(idx));
    	BizInfo[bizid][bText] = CreateDynamic3DTextLabel(string, COLOR_WHITE, BizInfo[bizid][bX], BizInfo[bizid][bY], BizInfo[bizid][bZ]+0.3, 15);
	    format(string, sizeof(string), "Administración: %s ha cambiado la ubicación del ID de empresa %d.", RPN(playerid), bizid);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/business.log", string);
	}
	else if(!strcmp(params, "precio", true, 5))
	{
	    if(sscanf(params, "s[32]ii", params, bizid, input)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit precio [bizid] [precio]");
		if(!BizInfo[bizid][bType]) return SendClientMessage(playerid, COLOR_GREY, "Id de negocio invalido.");
		//if(strcmp("El Estado", BizInfo[bizid][bOwner])) return SendClientMessage(playerid, COLOR_GREY, "No puede editar el precio de las empresas propias.");
	    BizInfo[bizid][bPrice] = input;
	    format(string, sizeof(string), "Propiedad: %s\nNegocio Tipo: %s\nEstado: En Venta\nPrecio: $%d", BizInfo[bizid][bOwner], RBT(bizid), input);
	    UpdateDynamic3DTextLabelText(BizInfo[bizid][bText], COLOR_WHITE, string);
	    format(string, sizeof(string), "Administración: %s ha establecido ID de negocio %d precio a $%d.", RPN(playerid), bizid, input);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/business.log", string);
	}
	else if(!strcmp(params, "nivel", true, 5))
	{
	    // L1: 0 | L2: 100 | L3: 300 | L4: 700 | L5: 1200
	    if(sscanf(params, "s[32]ii", params, bizid, input)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit nivel [bizid] [nivel]");
    	if(!BizInfo[bizid][bType]) return SendClientMessage(playerid, COLOR_GREY, "Id de negocio invalido.");
    	if(input < 0 || input > 5) return SendClientMessage(playerid, COLOR_GREY, "Niveles entre 1 y 5.");
	    BizInfo[bizid][bLevel] = input;
	    if(input == 1) BizInfo[bizid][bSold] = 0;
	    if(input == 2) BizInfo[bizid][bSold] = 100;
	    if(input == 3) BizInfo[bizid][bSold] = 300;
	    if(input == 4) BizInfo[bizid][bSold] = 700;
	    if(input == 5) BizInfo[bizid][bSold] = 1200;
	    format(string, sizeof(string), "Administración: %s ha establecido ID de negocio %d nivel a %d.", RPN(playerid), bizid, input);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/business.log", string);
	}
	else if(!strcmp(params, "productos", true, 8))
	{
	    if(sscanf(params, "s[32]ii", params, bizid, input)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit productos [bizid] [cantidad]");
    	if(!BizInfo[bizid][bType]) return SendClientMessage(playerid, COLOR_GREY, "Id de negocio invalido.");
    	if(input < 0) return SendClientMessage(playerid, COLOR_GREY, "Los productos no pueden ser negativos.");
	    BizInfo[bizid][bProducts] = input;
	    format(string, sizeof(string), "Administración: %s ha establecido id. de negocio %d productos a %d.", RPN(playerid), bizid, input);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/business.log", string);
	}
	else if(!strcmp(params, "boveda", true, 5))
	{
	    if(sscanf(params, "s[32]ii", params, bizid, input)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit boveda [bizid] [cantidad]");
    	if(!BizInfo[bizid][bType]) return SendClientMessage(playerid, COLOR_GREY, "Id de negocio invalido.");
    	if(input < 0) return SendClientMessage(playerid, COLOR_GREY, "El dinero de la bóveda no puede ser negativo.");
	    BizInfo[bizid][bMoney] = input;
	    format(string, sizeof(string), "Administración: %s ha establecido una ID de negocio %d bóveda de dinero para $%d.", RPN(playerid), bizid, input);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/business.log", string);
	}
	else if(!strcmp(params, "puerta", true, 5))
	{
	    if(sscanf(params, "s[32]ii", params, bizid, input)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit puerta [bizid] [1/0]");
    	if(!BizInfo[bizid][bType]) return SendClientMessage(playerid, COLOR_GREY, "Id de negocio invalido.");
    	if(input != 0 && input!=1) return SendClientMessage(playerid, COLOR_GREY, "USO: /bedit puerta [bizid] [1/0]");
		BizInfo[bizid][bStatus] = input;
	    format(string, sizeof(string), "Administración: %s ha establecido una ID de negocio %d la puerta $%d.", RPN(playerid), bizid, input);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		Log("logs/business.log", string);
	}

	else if(!strcmp(params, "truckers", true, 8))
	{
   	    if(sscanf(params, "s[32]ii", params, bizid, input)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /bedit truckers [bizid] [type] (1=Accepting | 2=NotAccepting");
        if(!BizInfo[bizid][bAP])
		{
		    BizInfo[bizid][bAP] = 1;
		    format(string, sizeof(string), "Administración: %s has set business ID %d to accept truckers.", RPN(playerid), bizid);
			SendAdminMessage(COLOR_DARKRED, 1, string);
			Log("logs/business.log", string);
		}
		else
		{
		    BizInfo[bizid][bAP] = 0;
		    format(string, sizeof(string), "Administración: %s has set business ID %d to NOT accept truckers.", RPN(playerid), bizid);
			SendAdminMessage(COLOR_DARKRED, 1, string);
			Log("logs/business.log", string);
		}
	}
	return 1;
}

