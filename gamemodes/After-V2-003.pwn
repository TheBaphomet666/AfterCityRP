CMD:tabcheck(playerid, params[])
{
	new string[128], playerb;
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /tabcheck[playerid]");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
    if(Tabbed[playerb])
    {
	    format(string, sizeof(string), "%d) {FFFFFF}%s (Tabbed for %d seconds)", playerb, RPN(playerb), AltTabbed[playerb]);
	    SendClientMessage(playerid, COLOR_ORANGE, string);
    }
    else SendClientMessage(playerid, COLOR_GREY, "Player is not alt-tabbed.");
	return 1;
}

CMD:emisora(playerid, params[])
{
	new string[128];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER && GetPlayerState(playerid) != PLAYER_STATE_PASSENGER) return SendClientMessage(playerid, COLOR_GREY, "Usted no está en un vehículo!");
	if(!strcmp(params, "off", true, 4))
	{
        foreach(Player, i)
		{
			if(IsPlayerNearPlayer(playerid, i, 3))
			{
				StopAudioStreamForPlayer(i);
				PlayerInfo[playerid][pRadio] = 0;
			}
		}
    	format(string, sizeof(string), "* %s toca en la pantalla el simbolo de off y la radio se apaga", RPN(playerid));
  		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	    return 1;
	}
    SendClientMessage(playerid, COLOR_GREY, "Su radio ha comenzado a reproducir (/emisora off para dejar de reproducir música)");
	format(string, sizeof(string), "* %s sintoniza unos pocos nobs y la radio empieza a tocar", RPN(playerid));
  	SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
  	ShowDialog(playerid, 56);
    foreach(Player, i)
	{
		if(IsPlayerNearPlayer(playerid, i, 3))
		{
			PlayerInfo[i][pRadio] = 1;
		}
	}
	return 1;
}

