	}
	else if(!strcmp(params, "sms", true))
	{
		if(!SMSes[playerid])
	    {
	        SMSes[playerid] = 1;
	        SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has empezado a escuchar SMSes.");
	    }
	    else
	    {
	        SMSes[playerid] = 0;
	        SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has dejado de escuchar SMSes.");
	    }
	}
	else if(!strcmp(params, "pm", true))
	{
	    if(!PMs[playerid])
	    {
	        PMs[playerid] = 1;
	        SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has empezado a escuchar PMs.");
	    }
	    else
	    {
	        PMs[playerid] = 0;
	        SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has dejado de escuchar PMs.");
