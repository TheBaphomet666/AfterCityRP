CMD:robarnegocio(playerid, params[])
{
	#pragma unused params
	new idx = GetPlayerVirtualWorld(playerid)-100;
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(idx > 0 && idx < MAX_BIZ && BizInfo[idx][bType] != 1 && BizInfo[idx][bType] != 2 && BizInfo[idx][bType] != 3 && BizInfo[idx][bType] != 4 && BizInfo[idx][bType] != 5 && BizInfo[idx][bType] != 6 || idx < 1 || idx > MAX_BIZ) return SendClientMessage(playerid, COLOR_GREY, "No estás en un negocio.");
	if(IsACop(playerid) || IsFBI(playerid) || IsSASD(playerid) || IsNG(playerid)) return SendClientMessage(playerid, COLOR_GREY, "¿Pero qué intentas?.");
	if(g_remainingBizTicks[idx])
	{
	    new strRobbedInfo[128];
		new playerName[36];
		GetPlayerName(g_bizRobber[idx], playerName, 36);
		format(strRobbedInfo, sizeof(strRobbedInfo), "%s robó este negocio hace menos de 20 minutos.", playerName);
 		return SendClientMessage(playerid, COLOR_GREY, strRobbedInfo);
	}
	if(g_bizRobber[idx] == playerid) return SendClientMessage(playerid, COLOR_GREY, "Ya estás robando un negocio.");

 new bystandards = 0;
	for (new i = 0; i < MAX_PLAYERS; i++) {
	    if (IsPlayerConnected(i) && i != playerid && IsACop(i)) bystandards++;
	    if (IsPlayerConnected(i) && i != playerid && IsFBI(i)) bystandards++;
	}

	if (bystandards < 1) return SendClientMessage(playerid, COLOR_GREY, "Sería injusto robar sin policías.");

	g_robbizBag[playerid] = CreateObject(1550, 0.0, 0.0, 1000.0, 0.0, 0.0, 0.0);
	AttachObjectToPlayer(g_robbizBag[playerid], playerid, -0.2, -0.4, 0.0, 0.0, 1.5, 2);
	g_bizRobber[idx] = playerid;
	g_bIsRobbiz[playerid] = 1;
	g_remainingRobTicks[playerid] = 70;
	g_remainingBizTicks[idx] = 20*60;
	g_apprehendedCash[playerid] = (BizInfo[idx][bMoney] * 45) / 100;

	SendClientMessage(playerid, COLOR_RED, "Has comenzado a robar el negocio^");
	new warningMessage[64];
	format(warningMessage, sizeof(warningMessage), "cuidado! %s Está robando este lugar!", RPN(playerid));
	SendNearbyMessage(playerid, 15.0, warningMessage, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
    AddDodWantedLevel(playerid, "Robar Negocio");
    AddDodWantedLevel(playerid, "Robar Negocio");
	for (new i = 0; i < MAX_PLAYERS; i++)
	{
	    if (IsPlayerConnected(i))
	    {
	        if (IsACop(i) || IsFBI(i))
	        {
				new strLENotification[128];
				new bizNames[][] = {"24/7 Store","Tienda de ropa","Ammunation","Club","Agencia de publicidad","Restaurante de comida rápida"};
				format(strLENotification, 128, "Un sospechoso ha robado %s",bizNames[BizInfo[idx][bType]-1]);
				SendClientMessage(i, COLOR_BLUE, strLENotification);
				SetPlayerCheckpoint(i, BizInfo[idx][bX], BizInfo[idx][bY], BizInfo[idx][bZ], 2);
			}
		}
	}

	return 1;
}

