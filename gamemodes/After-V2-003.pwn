CMD:oferta(playerid, params[])
{
	new boxer, amount, string[128];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(GetPlayerInterior(playerid) != 5 || GetPlayerVirtualWorld(playerid) != 1) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro del gimnasio ganton.");
	if(!BoxingMatch) return SendClientMessage(playerid, COLOR_GREY, "No hay combate de boxeo en curso.");
	if(!BoxTime) return SendClientMessage(playerid, COLOR_GREY, "El combate de boxeo ya ha comenzado.");
	if(BidAmount[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Ya ha realizado su puja para este partido.");
	if(sscanf(params, "ii", boxer, amount)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /oferta [boxeador] [cantidad]");
	if(boxer < 1 || boxer > 2) return SendClientMessage(playerid, COLOR_GREY, "Los boxeadores tienen entre 1 y 2.");
	if(PlayerInfo[playerid][pMoney] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes el dinero suficiente.");
	if(amount < 500) return SendClientMessage(playerid, COLOR_GREY, "La oferta mínima es de $500.");
	BoxerBid[boxer] += amount;
	BidAmount[playerid] = amount;
	BidBoxer[playerid] = boxer;
	GiveDodMoney(playerid, -amount);
	format(string, sizeof(string), "Rin de boxeo:% s ha puesto una oferta de $%d en %s.", RPN(playerid), amount, RPN(Boxer[boxer]));
	SendGymMessage(COLOR_WHITE, string);
    return 1;
}

