	if(sscanf(params, "ui", playerb, amount)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /transferir [ID] [cantidad]");
	if(amount <= 0) return SendClientMessage(playerid, COLOR_GREY, "Cantidad de dinero invalida.");
	if(PlayerInfo[playerid][pLevel] < 3) return SendClientMessage(playerid, COLOR_GREY, "Necesitas ser nivel 3 para poder transferir dinero.");
