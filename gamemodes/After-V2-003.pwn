public OnPlayerDisconnect(playerid, reason)
{
	PlayersConnected--;
	// Bank Robbery
	if(BankRobber[playerid])
    {
	    new Rob = BankRobber[playerid]-1, string[128];
	    DisablePlayerCheckpoint(playerid);
		BankRobbersCount --;
		format(BankRobbers[Rob], MAX_PLAYER_NAME, "");
		DeliverMoney[playerid] = 0;
		//
		foreach(Player, i)
		{
		    if(BankRobber[i] || PlayerInfo[i][pFac] == 1)
		    {
				if(Rob == 0) TextDrawHideForPlayer(i, Textdraw1);
				else if(Rob == 1) TextDrawHideForPlayer(i, Textdraw2);
				else if(Rob == 2) TextDrawHideForPlayer(i, Textdraw3);
				else if(Rob == 3) TextDrawHideForPlayer(i, Textdraw4);
				else if(Rob == 4) TextDrawHideForPlayer(i, Textdraw5);
			}
		}
		format(string, sizeof(string), "** %s se ha desconectado y el robo ha fracasado. **", RPN(playerid));
		SendRobberyMessage(COLOR_LIGHTRED, string);
		SendCopMessage(COLOR_LIGHTRED, string);
		BankRobber[playerid] = 0;
		if(BankRobbersCount == 0)
		{
			TextDrawHideForAll(Textdraw0);
			TextDrawHideForAll(Textdraw1);
			TextDrawHideForAll(Textdraw2);
			TextDrawHideForAll(Textdraw3);
			TextDrawHideForAll(Textdraw4);
			TextDrawHideForAll(Textdraw5);
			BankRobbery = 0;
			RobberyStarted = 0;
			format(string, sizeof(string), "** El robo al banco ha terminado, $%d fueron salvados y $%d fueron asegurados. **", TotalStolen*10000, (TotalRobbers*10000-(TotalStolen*10000)));
			SendClientMessageToAll(COLOR_YELLOW, string);
			TotalRobbers = 0;
			new copsonline;
			foreach(Player, i)
			{
			    if(IsACop(i) || IsSASD(i)) copsonline++;
			}
			foreach(Player, i)
			{
			    if(IsACop(i) || IsSASD(i))
			    {
					new Saved = (TotalRobbers*10000-(TotalStolen*10000));
					GiveDodMoney(i, Saved/copsonline);
					format(string, sizeof(string), "** Has recibido tu parte del dinero asegurado. ($%d)", Saved/copsonline);
					SendClientMessage(i, COLOR_LIME, string);
			    }
			}
		}
    }
    RemovePlayerAttachedObject(playerid, 0);
    //capturing
    if(PointTimer[playerid])
        {
            new string[128];
            KillTimer(PointTimer[playerid]);
            PointTimer[playerid] = 0;
            format(string, sizeof(string), "%s se ha desconectado y ha fallado en su captura.",RPN(playerid));
			SendClientMessageToAll(COLOR_YELLOW, string);
        }
    //paintball
    if(PlayerPaintballing[playerid] == 1)
	{
	    ResetPlayerWeapons(playerid);
	    ResetDodWeapons(playerid);
	    SetPlayerPos(playerid,1081.17,-1696.78,13.55);
	    for(new w = 0; w < 13; w++)
		{
		    GiveDodWeapon(playerid, PlayerInfo[playerid][pWeapon][w], PlayerInfo[playerid][pWeaponAmmo][w]);
		}
	}
	// Boxing
	//
	Finrenta(playerid);
	new string[128];
	if(Boxing[playerid])
	{
        // Getting other boxer's ID
		new playerb = -1;
        foreach(Player, i)
        {
            if(Boxing[i] && i != playerid) playerb = i;
        }
        if(playerb != -1)
        {
            format(string, sizeof(string), "Ring de Box: La batalla de box ha terminado, %s ha ganado la batalla.", RPN(playerb));
    		SendGymMessage(COLOR_WHITE, string);
            // Putting them out of the ring
            SetPlayerPos(playerb, 760.9759,4.7187,1000.7084);
            SetPlayerFacingAngle(playerb, 268.9407);
            SetPlayerPos(playerid, 760.9759,6.1794,1000.7093);
            SetPlayerFacingAngle(playerid, 268.9407);
            // Giving the winner extra skill point
            new oldj = strval(RPJL(playerb, JOB_BOXER));
            PlayerInfo[playerb][pJobSkill][JOB_BOXER] ++;
			new newj = strval(RPJL(playerb, JOB_BOXER));
		    if(oldj < newj)
			{
			    format(string, sizeof(string), "** Tu nivel de boxeador es ahora %d, ahora puedes causar %d mas de daño. **", newj, (newj*2)-2);
				SendClientMessage(playerb, COLOR_YELLOW, string);
			}
			//
			SendClientMessage(playerb, COLOR_LIME, " Has ganado la batalla de box.");
			SendClientMessage(playerid, COLOR_LIGHTRED, " Has perdido la batalla de box.");
			// Giving Bids
			new winner = pBoxer[playerb];
			foreach(Player, i)
			{
			    if(BidBoxer[i] == winner)
			    {
			        new percent, profit;
			        percent = (BidAmount[i]/BoxerBid[winner]) * 100;
			        profit = (percent * BoxerBid[pBoxer[playerid]]) / 100;
			        GiveDodMoney(i, BidAmount[i] + profit);
			        format(string, sizeof(string), " Has ganado $%d de apuestas, el dinero que apostaste se te ha regresado también.", profit);
			        SendClientMessage(i, COLOR_LIME, string);
			    }
				BidAmount[i] = 0;
				BidBoxer[i] = 0;
			}
			for(new w = 0; w < 13; w++)
			{
			    GiveDodWeapon(playerb, PlayerInfo[playerb][pWeapon][w], PlayerInfo[playerb][pWeaponAmmo][w]);
			}
        }
        for(new w = 0; w < 13; w++)
		{
		    GiveDodWeapon(playerid, PlayerInfo[playerid][pWeapon][w], PlayerInfo[playerid][pWeaponAmmo][w]);
		}
        // Resetting Values
		BoxingMatch = 0;
		Boxer[1] = -1;
		pBoxer[playerb] = 0;
		Boxer[2] = -1;
		pBoxer[playerid] = 0;
		Boxing[playerb] = 0;
		Boxing[playerid] = 0;
		BoxerBid[1] = 0;
		BoxerBid[2] = 0;
	    if(BoxTime)
	    {
			TogglePlayerControllable(playerb, 1);
	    	KillTimer(BoxTimer);
	    }
    }
	// The rest
	if(IsPlayerLoggedIn(playerid)) {SaveChar(playerid);}
	switch(reason)
	{
	    case 0: format(string, sizeof(string), "* %s ha salido del servidor. (AFK)", RPN(playerid));
	    case 1: format(string, sizeof(string), "* %s ha salido del servidor. (Salida normal)", RPN(playerid));
	    case 2: format(string, sizeof(string), "* %s ha salido del servidor. (Kickeado/Baneado)", RPN(playerid));
	}
	SendAdminMessage(COLOR_YELLOW, 1, string);
	foreach(Player, i)
	{
		if(JoinMessages[i])
		{
		    SendClientMessage(i, COLOR_LIGHTBLUE, string);
		}
	}
	/*new leaveMsg[128];
	format(leaveMsg, sizeof(leaveMsg), "02[%d] 03*** %s has left the server.", playerid, RPN(playerid));
	IRC_GroupSay(groupID, IRC_CHANNEL, leaveMsg); */
	foreach(Player, i)
	{
	    if(Specid[i] == playerid)
	    {
	        Spec[i] = 0;
			Specid[i] = -1;
	    	TogglePlayerSpectating(i, 0);
			SetPlayerVirtualWorld(i, PlayerInfo[playerid][pVW]);
			SetPlayerInterior(i, PlayerInfo[playerid][pInt]);
			SetPlayerPos(i, PlayerInfo[playerid][pX], PlayerInfo[playerid][pY], PlayerInfo[playerid][pZ]);
			SendClientMessage(i, COLOR_WHITE, " Has dejado de spectar.");
	    }
	}
	// Clearing AD
	for(new i=0; i<MAX_ADS; i++)
	{
		if(strfind(AD[i], RPN(playerid)) != -1)
   		{
   		    format(AD[i], 128, "");
   		}
   	}
	// End of Clearing AD
	ClearChar(playerid);
	return 1;
}


