	        if(Live[i] && i != playerid) playerb = i;
	    }
	    Live[playerid] = 0;
	    Live[playerb] = 0;
	    LiveConference = 0;
	    SendClientMessage(playerid, COLOR_LIGHTRED, "Ha terminado la conferencia de prensa en vivo.");
	    SendClientMessage(playerb, COLOR_LIGHTRED, "La conferencia de prensa en vivo ha terminado.");
	}
	else
	{
	    if(LiveRequest[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Ya has ofrecido a alguien una conferencia de prensa en vivo.");
	    if(LiveConference) return SendClientMessage(playerid, COLOR_GREY, "Otra conferencia de prensa en vivo ya se está celebrando.");
		if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /envivo [playerid]");
		if(playerb == playerid) return SendClientMessage(playerid, COLOR_GREY, "No se puede ofrecer a una conferencia de prensa en vivo.");
		if(LiveBy[playerb] != -1) return SendClientMessage(playerid, COLOR_GREY, "Jugador ya se ha ofrecido una conferencia de prensa en vivo.");
		LiveBy[playerb] = playerid;
		LiveRequest[playerid] = 1;
		format(string, sizeof(string), " Has ofrecido %s una conferencia de prensa en vivo.", RPN(playerb));
		SendClientMessage(playerid, COLOR_LIME, string);
		format(string, sizeof(string), "Reportero de Noticia %s te ha ofrecido una conferencia de prensa en vivo. (/aceptar entrevista) (/cancelar entrevista)", RPN(playerid));
		SendClientMessage(playerb, COLOR_LIME, string);
	}
	return 1;
}

