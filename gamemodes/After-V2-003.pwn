	if(sscanf(params, "i", id)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /deletebiz [bizid]");
	if(!BizInfo[id][bType]) return SendClientMessage(playerid, COLOR_GREY, "ID de empresa no válido.");
    foreach(Player, i)
	{
	    if(IsPlayerLoggedIn(i) && PlayerInfo[i][pBiz] == id)
		{
		    PlayerInfo[playerid][pBiz] = 0;
		    format(string, sizeof(string), " Administrador %s ha eliminado tu negocio.", RPN(playerid));
			SendClientMessage(i, COLOR_WHITE, string);
		}
	}
	if(!strcmp("El Estado", BizInfo[id][bOwner]))
	{
		format(string, sizeof(string), "Administración: %s ha eliminado el negocio ID %d.", RPN(playerid), id);
	}
	else
	{
	    format(string, sizeof(string), "Administración: %s ha eliminado el negocios de %s. (ID %d)", RPN(playerid), BizInfo[id][bOwner], id);
	}
	SendAdminMessage(COLOR_DARKRED, 1, string);
	Log("logs/business.log", string);
	BizInfo[id][bType] = 0;
    BizInfo[id][bStatus] = 0;
    format(BizInfo[id][bOwner], 32, "");
    BizInfo[id][bX] = 0;
    BizInfo[id][bY] = 0;
    BizInfo[id][bZ] = 0;
    BizInfo[id][bMoney] = 0;
    BizInfo[id][bProducts] = 0;
    DestroyDynamicPickup(BizInfo[id][bPickup]);
    DestroyDynamic3DTextLabel(BizInfo[id][bText]);
	return 1;
}

