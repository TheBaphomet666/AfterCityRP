CMD:haceradmintemp(playerid, params[])
{
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pAdmin] >= 6 || IsPlayerAdmin(playerid))
	{
	    new playerb, admin, time, string[364];
	    if(sscanf(params, "uii", playerb, admin, time)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /haceradmintemp [ID] [nivel] [tiempo]");
		if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "ID de usuario invalido.");
		if(PlayerInfo[playerb][pAdmin]>=PlayerInfo[playerid][pAdmin]) return SendClientMessage(playerid, COLOR_RED, "Estás molestando con fuerzas que no conoces");
		if (admin>=PlayerInfo[playerid][pAdmin])return SendClientMessage(playerid, COLOR_RED, "Te falta poder");
	    if(PlayerInfo[playerb][pAdmin] != admin )
	    {
	        new log[364];
		    if(PlayerInfo[playerb][pAdmin] < admin)
			{
		    	format(string, sizeof(string), "Administración: %s ha sido {33AA33}temporalmente promovido {AA3333}%s a nivel {33AA33}%d {AA3333}de admin por %d minutos.", RPN(playerid), RPN(playerb), admin, time);
            	format(log, sizeof(log), "Administración: %s (%s) promovio temporalmente a %s (%s) a nivel %d de admin por %d minutos.", RPN(playerid), RPIP(playerid), RPN(playerb), RPIP(playerb), admin, time);
			}
			else if(PlayerInfo[playerb][pAdmin] > admin)
			{
  				format(string, sizeof(string), "Administración: %s fue {FF9900}temporalmente degradado {AA3333}%s a nivel {33AA33}%d {AA3333}de admin por %d minutos.", RPN(playerid), RPN(playerb), admin, time);
            	format(log, sizeof(log), "Administración: %s (%s) ha {FF9900}degradado temporalmente a %s (%s) a nivel %d de admin por %d minutos.", RPN(playerid), RPIP(playerid), RPN(playerb), RPIP(playerb), admin, time);
			}
			PlayerInfo[playerb][pOldAdmin] = PlayerInfo[playerb][pAdmin];
			Log("logs/makeadmin.log", log);
			PlayerInfo[playerb][pAdmin] = admin;
			PlayerInfo[playerb][pSafeAdmin] = 1;
			SetTimerEx("SetBack", 60000 * time, false, "i", playerb);
			PlayerInfo[playerb][pSafeAdmin] = 1;
			SendAdminMessage(COLOR_DARKRED, 1, string);
			if(!PlayerInfo[playerb][pAdmin])
			{
			    PlayerInfo[playerb][pSafeAdmin] = 0;
			    PlayerInfo[playerid][pGangMod] = 0;
			    PlayerInfo[playerid][pFacMod] = 0;
			    PlayerInfo[playerid][pBanAppealer] = 0;
			    SendClientMessage(playerb, COLOR_DARKRED, string);
			}
			new file[32];
			format(file, sizeof(file), "users/%s.ini", RPNU(playerb));
			format(file, sizeof(file), "users/%s.ini", dini_Get(file, "AdminName"));
			if(dini_Exists(file))
			{
				dini_IntSet(file, "Admin", PlayerInfo[playerb][pAdmin]);
			}
			format(file, sizeof(file), "users/%s.ini", RPNU(playerb));
			if(dini_Int(file, "AdminAccount") == 1)
			{
			    format(file, sizeof(file), "users/%s.ini", dini_Get(file, "OldName"));
			    dini_IntSet(file, "Admin", PlayerInfo[playerb][pAdmin]);
			}
		}
		else SendClientMessage(playerid, COLOR_GREY, "El jugador ya tiene este nivel de admin.");
	}
	else
	{
	    SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	}
	return 1;
}

