stock GetVehicleDriver(vid)
{
     for(new i; i < GetMaxPlayers(); i++)
     {
          if(!IsPlayerConnected(i)) continue;
          if(GetPlayerVehicleID(i) == vid && GetPlayerVehicleSeat(i) == 0) return i;
     }
     return INVALID_PLAYER_ID;
}
