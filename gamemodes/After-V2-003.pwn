CMD:renunciarempleo(playerid, params[])
{
	new string[128], jobid;
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!PlayerInfo[playerid][pJob] && PlayerInfo[playerid][pVIP] < 2) return SendClientMessage(playerid, COLOR_GREY, "no tienes un trabajo.");
 	if(PlayerInfo[playerid][pVIP] >= 2)
	{
		if(sscanf(params, "is[8]", jobid, params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /renunciarempleo [1-2] [confirmar]");
		if(jobid == 1 && !PlayerInfo[playerid][pJob]) return SendClientMessage(playerid, COLOR_GREY, "No tienes un trabajo en este espacio.");
		if(jobid == 2 && !PlayerInfo[playerid][pVIPJob]) return SendClientMessage(playerid, COLOR_GREY, "No tienes un trabajo en este espacio.");
	}
	else if(sscanf(params, "s[8]", params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /renunciarempleo [confirm]");
	if(!strcmp(params, "confirm", true))
	{
	    if(PlayerInfo[playerid][pVIP] >= 2)
	    {
	        if(jobid == 1)
	        {
		        format(string, sizeof(string), "* has dejado  {FF6347}%s {33CCFF}trabajo.", RPJN(playerid));
				SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		    	PlayerInfo[playerid][pJob] = JOB_NONE;
	        }
			else if(jobid == 2)
			{
		        format(string, sizeof(string), "* has dejado {FF6347}%s {33CCFF}trabajo.", RPVJN(playerid));
				SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		    	PlayerInfo[playerid][pVIPJob] = JOB_NONE;
			}
	        return 1;
	    }
	    format(string, sizeof(string), "* has dejado {FF6347}%s {33CCFF}trabajo.", RPJN(playerid));
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	    PlayerInfo[playerid][pJob] = JOB_NONE;
	}
	return 1;
}

CMD:mejorarcasa(playerid, params[])
{
	new string[128], idx = PlayerInfo[playerid][pHouse], price;
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(!PlayerInfo[playerid][pHouse]) return SendClientMessage(playerid, COLOR_GREY, "No tienes casa.");
    if(HouseInfo[idx][hLevel] == 10 && PlayerInfo[playerid][pVIP] == 0) return SendClientMessage(playerid, COLOR_GREY, "No eres VIP");
	if(!PlayerInfo[playerid][pVHouse])
	{
	    if(!IsPlayerInRangeOfPoint(playerid, 2, HouseInfo[idx][hX], HouseInfo[idx][hY], HouseInfo[idx][hZ])) return SendClientMessage(playerid, COLOR_GREY, "No estás cerca de tu casa.");
	}
	else
	{
	    new done;
   	    if(!IsPlayerInRangeOfPoint(playerid, 2, HouseInfo[idx][hX], HouseInfo[idx][hY], HouseInfo[idx][hZ]))
 		{
	 		done ++;
	   	    idx = PlayerInfo[playerid][pVHouse];
	   	    if(!IsPlayerInRangeOfPoint(playerid, 2, HouseInfo[idx][hX], HouseInfo[idx][hY], HouseInfo[idx][hZ])) done ++;
		}
		if(done == 2) return SendClientMessage(playerid, COLOR_GREY, "No estas cerca de tu casa.");

	}
	if(sscanf(params, "s[8]", params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /mejorarcasa [confirmar]");
	if(HouseInfo[idx][hLevel] >= 11) return SendClientMessage(playerid, COLOR_GREY, "Ya no puedes mejorar más tu casa.");
	if(!strcmp(params, "confirm", true))
	{

	    if(HouseInfo[idx][hLevel] == 1) price = 15000;
	    else if(HouseInfo[idx][hLevel] == 2) price = 30000;
	    else if(HouseInfo[idx][hLevel] == 3) price = 60000;
	    else if(HouseInfo[idx][hLevel] == 4) price = 70000;
	    else if(HouseInfo[idx][hLevel] == 5) price = 80000;
	    else if(HouseInfo[idx][hLevel] == 6) price = 90000;
	    else if(HouseInfo[idx][hLevel] == 7) price = 120000;
	    else if(HouseInfo[idx][hLevel] == 8) price = 160000;
	    else if(HouseInfo[idx][hLevel] == 9) price = 200000;
	    else if(HouseInfo[idx][hLevel] == 10) price = 300000;
        else if(HouseInfo[idx][hLevel] == 11) price = 400000;

		if(PlayerInfo[playerid][pMoney] < price)
		{
		    format(string, sizeof(string), "Necesitas $%d para mejorar tu casa.", price);
			SendClientMessage(playerid, COLOR_GREY, string);
			return 1;
		}
		HouseInfo[idx][hLevel] ++;
		GiveDodMoney(playerid, -price);
        HouseInfo[idx][hPrice] += price;
		format(string, sizeof(string), " Has mejorado tu casa al nivel %d.", HouseInfo[idx][hLevel]);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "{00C0FF}Propiedad: %s\nTipo: %s\nEstado: %s", HouseInfo[idx][hOwner], HT(idx), RHS(idx));
		UpdateDynamic3DTextLabelText(HouseInfo[idx][hText], COLOR_WHITE, string);
	}
	return 1;
}
