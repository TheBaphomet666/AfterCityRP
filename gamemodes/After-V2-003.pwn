CMD:sellfish(playerid, params[])
{
	new idx = GetPlayerVirtualWorld(playerid)-100, string[128], fish, price;
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(idx > 0 && idx < MAX_BIZ && BizInfo[idx][bType] != 1 || idx < 1 || idx > MAX_BIZ) return SendClientMessage(playerid, COLOR_GREY, "You are not inside a 24/7 Market.");
	if(sscanf(params, "i", fish)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /sellfish [fishslot]");
	if(fish < 1 || fish > 5) return SendClientMessage(playerid, COLOR_GREY, "Los peces están entre 1 y 5.");
	fish = fish-1;
	if(!PlayerInfo[playerid][pFish][fish]) return SendClientMessage(playerid, COLOR_GREY, "No tienes ningún pescado en esta ranura.");
	if(PlayerInfo[playerid][pFish][fish] < 20) return SendClientMessage(playerid, COLOR_GREY, "Usted no puede vender peces que pesan menos de 20 libras.");
	price = PlayerInfo[playerid][pFish][fish] * 2;
	GiveDodMoney(playerid, price);
	PlayerInfo[playerid][pFish][fish] = 0;
	format(string, sizeof(string), "* %s ha vendido un pescado al mercado 24/7.", RPN(playerid));
 	SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	format(string, sizeof(string), "* Ha vendido un pescado {FF6347}%d lbs {33CCFF} para {FF6347} $%d {33CCFF}.", price/2, price);
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	return 1;
}

