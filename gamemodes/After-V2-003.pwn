public Bombing(playerid)
{
	CreateExplosion(BombX[playerid],BombY[playerid],BombZ[playerid],2,Bomb_Radius);
	CreateExplosion(BombX[playerid]+0.4,BombY[playerid]+0.4,BombZ[playerid],2,Bomb_Radius);
	CreateExplosion(BombX[playerid]-0.4,BombY[playerid]-0.4,BombZ[playerid],2,Bomb_Radius);
	CreateExplosion(BombX[playerid]+0.4,BombY[playerid]-0.4,BombZ[playerid],2,Bomb_Radius);
	CreateExplosion(BombX[playerid]-0.4,BombY[playerid]+0.4,BombZ[playerid],2,Bomb_Radius);
	BombX[playerid] = 0;
	BombY[playerid] = 0;
	BombZ[playerid] = 0;
	PlantedBomb[playerid] = 0;
	DestroyObject(PlayerBombObject[playerid]);
    Delete3DTextLabel(PlayerBombText[playerid]);
    Msg(playerid,COLOR_NOTIFICATION,"Ha explotado!");
	return true;
}

