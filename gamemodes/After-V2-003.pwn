CMD:duda(playerid,params[])
{
    new mensaje[256];
    if(sscanf(params,"s[256]",mensaje)) return SendClientMessage(playerid,-1,"Utiliza /duda [texto]");
    if(AntiAdv(playerid, params)) return 1;
    if(HelpTime[playerid] > 0)
    {
	    format(mensaje, sizeof(mensaje), "Necesitas esperar %d segundos para enviar otra duda.", HelpTime[playerid]);
		SendClientMessage(playerid, COLOR_GREY, mensaje);
		return 1;
	}
    if(CanalDBloqueado == 0 )
    {
    format(mensaje,sizeof(mensaje),"Duda de %s[ID:%d]: %s",RPN(playerid),playerid,mensaje);
    SenddudaMessage(COLOR_YELLOW, mensaje);
    format(mensaje,sizeof(mensaje),"Para responder /re %d",playerid);
    SenddudaMessage(COLOR_YELLOW, mensaje);
    Log("logs/hreports.log", mensaje);
    SendClientMessage(playerid,-1,"Has enviado tu duda a los adminsitradores. Espera su respuesta");
    HelpTime[playerid] = 25;
	SetTimerEx("HelpTimer", 60000, false, "i", playerid);
    DudaEnviada[playerid] = 1;
    }
    else SendClientMessage(playerid,-1,"El canal de dudas está desactivado");
    
    return 1;
}

CMD:re(playerid,params[])
{
    if(PlayerInfo[playerid][pAdmin] <1 && !PlayerInfo[playerid][pHelper] ) return SendClientMessage(playerid,-1,"No estás autorizado para usar este comando");
    new jugador,mensaje[256];
    if(sscanf(params,"us[256]",jugador,mensaje)) return SendClientMessage(playerid,-1,"Utiliza /re [ID/ParteDelNombre] [texto]");
    if(DudaEnviada[jugador] == 1)
    {
        format(mensaje,sizeof(mensaje),"Respuesta de %s a %s: %s",RPN(playerid), RPN(jugador),mensaje);
        SendClientMessage(jugador,-1,mensaje);
        SenddudaMessage(COLOR_YELLOW, mensaje);
        format(mensaje,sizeof(mensaje),"Respuesta enviada al jugador %s",RPN(jugador));
        SendClientMessage(playerid,-1,mensaje);
        DudaEnviada[jugador] = 0;
        PlayerInfo[playerid][pADuda]++;
    }
    else SendClientMessage(playerid,-1,"Ese jugador no ha enviado ninguna duda");
    return 1;
}

