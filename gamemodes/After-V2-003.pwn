	    		format(string, sizeof(string), "{FF6347}Paga: {FFFFFF}$%d  {FF6347}Impuestos: {FFFFFF}$%d (%d porciento)  {FF6347}Paga total: {FFFFFF}$%d", paycheck, taxamount, TaxPercent, finalpaycheck);
				SendClientMessage(i, COLOR_FADE1, string);
	    		format(string, sizeof(string), "{FF6347}Balance anterior: {FFFFFF}$%d  {FF6347}Paga de facción: {FFFFFF}$%d {FF6347}Nuevo balance: {FFFFFF}$%d", oldbalance, strval(FACPAY(i)), PlayerInfo[i][pBank]);
	    		SendClientMessage(i, COLOR_FADE1, string);
	    		if(PlayerInfo[i][pMinutes] >= 120)
				{
					format(string, sizeof(string), "* Has recibido %d puntos de respeto por jugar %d minutos.", strval(stack), PlayerInfo[i][pMinutes]);
