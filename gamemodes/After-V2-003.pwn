CMD:mascara(playerid, params[])
{
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(PlayerInfo[playerid][pMask] != 1) return SendClientMessage(playerid, COLOR_GREY, "Usted necesita comprar una máscara de un 24/7!.");
    if(PlayerInfo[playerid][pMasked] == 0)
    {
        PlayerInfo[playerid][pMasked] = 1;
