CMD:captura(playerid, params[])
{
	new string[128], done;
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pFamRank] < 5) return SendClientMessage(playerid, COLOR_GREY, "No eres un líder de familia.");
	for(new idx = 0; idx < MAX_POINTS; idx++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 15, PointInfo[idx][pX], PointInfo[idx][pY], PointInfo[idx][pZ]))
		{
            if(PointInfo[idx][pTime] > 1) return SendClientMessage(playerid, COLOR_GREY, "Por favor espere hasta que acabe el enfriamineto del punto.");
		    if(Capturing) return SendClientMessage(playerid, COLOR_GREY, "Este punto ya está siendo capturado.");
		    format(string, sizeof(string), " %s Está capturando el punto %s.", RPN(playerid), PointInfo[idx][pName]);
		    SendClientMessageToAll (COLOR_ORANGE, string);
		    SendClientMessage(playerid, COLOR_LIGHTBLUE, "No te muevas por 5 segundos para empezar la captura.");
            Capturing = 1;
            GetPlayerPos(playerid, Capture[0], Capture[1], Capture[2]);
            Capturer = playerid;
            CapturerFam = idx;
			SetTimerEx("CapturePoint", 5000, false, "dd", Capturer, CapturerFam);
		    done = 1;
		    break;
		}
    }
    if(!done) SendClientMessage(playerid, COLOR_GREY, "No estás cerca de un punto de captura.");
	return 1;
}

CMD:frango(playerid, params[])
{
	new playerb, rank, string[128], idx = PlayerInfo[playerid][pFam];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pFamRank] < 5) return SendClientMessage(playerid, COLOR_GREY, "No eres un líder de familia.");
	if(sscanf(params, "ui", playerb, rank)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /frango [playerid] [rango]");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "You can't change your rank.");
	if(PlayerInfo[playerid][pFamRank] < PlayerInfo[playerb][pFamRank]) return SendClientMessage(playerid, COLOR_GREY, "No puedes degradar a los miembros más altos.");
    if(PlayerInfo[playerid][pFamRank] == PlayerInfo[playerb][pFamRank] && strcmp(RPN(playerid), FamInfo[idx][fLeader], false)) return SendClientMessage(playerid, COLOR_GREY, "You can't promote/demote same ranked members.");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(PlayerInfo[playerb][pFam] != PlayerInfo[playerid][pFam]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no está en tu familia.");
	if(rank < 1 || rank > 6) return SendClientMessage(playerid, COLOR_GREY, "Los rangos están entre 1 y 6.");
	if(PlayerInfo[playerb][pFamRank] == rank) return SendClientMessage(playerid, COLOR_GREY, "El jugador ya tiene ese rango.");
	if(PlayerInfo[playerb][pFamRank] < rank)
	{
	    PlayerInfo[playerb][pFamRank] = rank;
		format(string, sizeof(string), "ha sido {33AA33}promovido {33CCFF}%s a {33AA33}%s (%d){33CCFF}.", RPN(playerb), RPFaRN(playerb), rank);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "%s te ha {33AA33}promovido {33CCFF}a ti {33AA33}%s (%d){33CCFF}.", RPN(playerid), RPFaRN(playerb), rank);
		SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	}
	else if(PlayerInfo[playerb][pFamRank] > rank)
	{
	    PlayerInfo[playerb][pFamRank] = rank;
		format(string, sizeof(string), "ha sido {FF9900}degradado {33CCFF}%s a {FF9900}%s (%d){33CCFF}.", RPN(playerb), RPFaRN(playerb), rank);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "%s te ha {FF9900}degradado {33CCFF}a ti {FF9900}%s (%d){33CCFF}.", RPN(playerid), RPFaRN(playerb), rank);
		SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	}
	return 1;
}

