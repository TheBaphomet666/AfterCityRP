	SendRobberyMessage(COLOR_LIGHTGREEN, string);
	RobberyRequest[playerid] = 1;
	return 1;
}


CMD:aceptarrobo(playerid, params[])
{
	new string[128], playerb;
    if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!BankRobbery) return SendClientMessage(playerid, COLOR_GREY, "No hay ningún robo de banco que se está configurando.");
	if(BankRobber[playerid] != 1) return SendClientMessage(playerid, COLOR_GREY, "Usted no es el líder de robo de banco.");
	if(BankRobbersCount >= 5) return SendClientMessage(playerid, COLOR_GREY, "Usted no puede tener más de 5 jugadores robando el banco.");
	if(RobberyStarted) return SendClientMessage(playerid, COLOR_GREY, "El robo del banco ya ha comenzado.");
	if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /acetarrobo [playerid]");
	if(!RobberyRequest[playerb]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no ha solicitado inscribirse en el robo.");
	RobberyRequest[playerb] = 0;
	BankRobbersCount ++;
	BankRobber[playerb] = BankRobbersCount;
	format(BankRobbers[BankRobbersCount-1], MAX_PLAYER_NAME, "%s", RPN(playerb));
	format(string, sizeof(string), "Robo de banco: %s ha aceptado la solicitud de %s para unirse al robo de un banco.", RPN(playerid), RPN(playerb));
	SendRobberyMessage(COLOR_LIGHTGREEN, string);
	// Textdraw
	if(BankRobbersCount == 2) {TextDrawSetString(Textdraw2, BankRobbers[1]);}
	else if(BankRobbersCount == 3) {TextDrawSetString(Textdraw3, BankRobbers[2]);}
	else if(BankRobbersCount == 4) {TextDrawSetString(Textdraw4, BankRobbers[3]);}
	else if(BankRobbersCount == 5) {TextDrawSetString(Textdraw5, BankRobbers[4]);}
	foreach(Player, i)
	{
	    if(BankRobber[i])
	    {
	        TextDrawShowForPlayer(i, Textdraw0);
			TextDrawShowForPlayer(i, Textdraw1);
	    	if(BankRobbersCount >= 2) {TextDrawShowForPlayer(i, Textdraw2);}
			else if(BankRobbersCount >= 3) {TextDrawShowForPlayer(i, Textdraw3);}
			else if(BankRobbersCount >= 4) {TextDrawShowForPlayer(i, Textdraw4);}
			else if(BankRobbersCount >= 5) {TextDrawShowForPlayer(i, Textdraw5);}
	    }
	}
	return 1;
}

CMD:iniciarrobo(playerid, params[])
{
    if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
