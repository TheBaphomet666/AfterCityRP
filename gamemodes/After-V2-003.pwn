CMD:desbanear(playerid, params[])
{
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	new string[128];
	if(sscanf(params, "s[32]", params)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /desbanear [nombre]");
	new file[64], IP[16];
	format(file, sizeof(file), "users/%s.ini", params);
	if(dini_Exists(file))
	{
	    if(dini_Int(file, "pBanned") == 0) return SendClientMessage(playerid, COLOR_GREY, "El jugador no esta baneado.");
		format(IP, sizeof(IP), "%s", dini_Get(file, "IP"));
	    // Userfile
	    dini_IntSet(file, "pBanned", 0);
	    dini_IntSet(file, "Warns", 0);
	    dini_Set(file, "Warn1", "");
	    dini_Set(file, "Warn2", "");
	    dini_Set(file, "Warn3", "");
	    dini_Set(file, "BanReason", "");
	    dini_Set(file, "BannedBy", "");
	    format(string, sizeof(string), "Administración: %s desbaneo a %s.", RPN(playerid), params);
		SendAdminMessage(COLOR_DARKRED, 1, string);
		format(string, sizeof(string), "Administración: %s (%s) desbaneo a %s (%s).", RPN(playerid), RPIP(playerid), params, IP);
		Log("logs/unban.log", string);
		return cmd_desbanearip(playerid, IP);
	}
	else
	{
	    SendClientMessage(playerid, COLOR_GREY, "Nombre de usuario no encontrado.");
	}

	return 1;
}

CMD:desbanearip(playerid, params[])
{
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pAdmin] < 7) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	new string[128];
	if(sscanf(params, "s[16]", params)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /desbanearip [ip]");
	if(IsValidIP(params)) return SendClientMessage(playerid, COLOR_GREY, "Dirección IP invalida.");
	new File:ban = fopen("ban.cfg", io_read);
	new idx = 1, done;
	while(fread(ban, string))
	{
		if(!strcmp(params, string, true, strlen(params)))
		{
			fdeleteline("ban.cfg", idx);
			format(string, sizeof(string), "Administración: %s desbaneo la IP %s", RPN(playerid), params);
			SendAdminMessage(COLOR_DARKRED, 1, string);
			format(string, sizeof(string), "Administración: %s (%s) desbaneo la IP %s", RPN(playerid), RPIP(playerid), params);
			Log("logs/unban.log", string);
			done = 1;
	    }
		idx++;
	}
	fclose(ban);
	if(!done) SendClientMessage(playerid, COLOR_GREY, "La IP no esta baneada.");
	return 1;
}


