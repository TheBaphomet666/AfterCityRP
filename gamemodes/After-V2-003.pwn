CMD:bimpuestos(playerid, params[])
{
	new string[128];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!IsAGov(playerid) || !PlayerInfo[playerid][pFacLeader]) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!IsPlayerNearBankBooth(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No está cerca de un banco.");
    //if(GetPlayerVirtualWorld(playerid) != 27934) return SendClientMessage(playerid, COLOR_GREY, "You are not inside the Tax Vault.");
	format(string, sizeof(string), "[Declaración bancaria]: La bóveda de impuestos del estado contiene $%d.", TaxMoney);
	SendClientMessage(playerid, COLOR_DARKRED, string);
	return 1;
}

CMD:retirarimpuestos(playerid, params[])
{
	new amount, string[128];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!IsAGov(playerid) || !PlayerInfo[playerid][pFacLeader]) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(!IsPlayerNearBankBooth(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No está cerca de un banco.");
	//if(GetPlayerVirtualWorld(playerid) != 27934) return SendClientMessage(playerid, COLOR_GREY, "You are not inside the Tax Vault.");
	if(sscanf(params, "i", amount)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /retirarimpuestos [cantidad]");
	if(amount > TaxMoney) return SendClientMessage(playerid, COLOR_GREY, "La bóveda de impuestos no tiene tanto dinero dentro de ella.");
	if(amount <= 0)  return SendClientMessage(playerid, COLOR_GREY, "Monto de dinero no válido.");
	TaxMoney -= amount;
	GiveDodMoney(playerid, amount);
