CMD:usar(playerid, params[])
{
	new string[128];
	if(sscanf(params, "s[8]", params))
	{
		SendClientMessage(playerid, COLOR_WHITE, "Utiliza /usar [opción]");
		SendClientMessage(playerid, COLOR_GREY, "Opciones: Cigarro | Marihuana | Cocaina");
		return 1;
	}
	if(IsPlayerCuffed(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No Puedes hacer eso ahora mismo.");
	if(!strcmp(params, "cigarro", true))
	{
	    if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No Puedes hacer eso ahora mismo.");
		if(!PlayerInfo[playerid][pCigar]) return SendClientMessage(playerid, COLOR_GREY, "Usted no tiene ningún cigarro.");
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_SMOKE_CIGGY);
		PlayerInfo[playerid][pCigar] --;
		format(string, sizeof(string), "* %s saca un cubo de un cigarro cubano y lo enciende y lo enciende.", RPN(playerid));
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	}
	else if(!strcmp(params, "marihuana", true))
	{
	    //if(GetPlayerDrunkLevel(playerid) >= 5000) return SendClientMessage(playerid, COLOR_GREY, "You are too drunk/stoned to use drugs.");
    	if(PlayerInfo[playerid][pStoned] >= 5) return SendClientMessage(playerid, COLOR_GREY, "You are too stoned to use drugs.");
	    new Float:H;
	    if(!PlayerInfo[playerid][pWeed]) return SendClientMessage(playerid, COLOR_GREY, "No tienes hierba.");
	    if(DrugTimeout[playerid]) return SendClientMessage(playerid, COLOR_GREY, " No puedes usar drogas en este momento.");
	    GetPlayerHealth(playerid, H);
	    if(H >= 100) return SendClientMessage(playerid, COLOR_GREY, "Ya tienes salud completa.");
	    // Animation
	    if(!IsPlayerInAnyVehicle(playerid)) ApplyAnimation(playerid,"Fumando","M_smkstnd_loop",8,0,0,0,0,0,1);
		else ApplyAnimation(playerid,"ped","Smoke_in_car",2.1,0,0,0,0,0,1);
		// Effects
	    SetPlayerHealth(playerid, H+10);
	    GetPlayerHealth(playerid, H);
	    if(H > 100) SetPlayerHealth(playerid, 100);
	    PlayerInfo[playerid][pWeed] --;
