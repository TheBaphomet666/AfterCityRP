CMD:vendermats(playerid, params[])
{
	new string[128], playerb, amount, price;
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pJob] != JOB_WSMUGGLER && PlayerInfo[playerid][pVIPJob] != JOB_WSMUGGLER) return SendClientMessage(playerid, COLOR_GREY, "Usted no es un contrabandista/traficante de armas.");
	if(sscanf(params, "uii", playerb, amount, price)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /vendermats [playerid] [cantidad] [precio]");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes venderte materiales a ti mismo.");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(PlayerInfo[playerid][pLevel] < 2) return SendClientMessage(playerid, COLOR_GREY, "Debe tener al menos nivel 2 para vender materiales.");
	if(!IsPlayerNearPlayer(playerid, playerb, 2)) return SendClientMessage(playerid, COLOR_GREY, "Estás muy lejos de ese jugador.");
	if(PlayerInfo[playerid][pMaterials] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes mucho material en ti.");
	if(SellMaterialsTo[playerid] == playerb) return SendClientMessage(playerid, COLOR_GREY, "Usted ya ha ofrecido a ese jugador para venderles materiales.");
	if(price <= 0) return SendClientMessage(playerid, COLOR_GREY, "Precio de materiales no válido.");
	// Clearing old buyer
	foreach(Player, i)
	{
		if(IsPlayerLoggedIn(i))
		{
		    if(SellMaterialsTo[playerid] == i && BuyMaterialsFrom[i] == playerid)
		    {
		        BuyMaterialsFrom[i] = -1; BuyMaterialsAmount[i] = 0; BuyMaterialsPrice[i] = 0; BeenOfferedMaterials[i] = 0;
		    }
		}
	}
	// New buyer
	SellMaterialsTo[playerid] = playerb;
	BuyMaterialsFrom[playerb] = playerid;
	BuyMaterialsAmount[playerb] = amount;
	BuyMaterialsPrice[playerb] = price;
	BeenOfferedMaterials[playerb] = 1;
	format(string, sizeof(string), " Usted ha ofrecido %s para venderles %d materiales para $%d.", RPN(playerb), amount, price);
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " %s ha ofrecido venderle %d materiales para $%d. (/aceptar materiales) (/cancelar materiales)", RPN(playerid), amount, price);
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	return 1;
}

