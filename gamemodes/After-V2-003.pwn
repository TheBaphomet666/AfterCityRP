CMD:confirobanco(playerid, params[])
{
	new string[128], cops;
    if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(IsACop(playerid) || IsAGov(playerid)) return SendClientMessage(playerid, COLOR_GREY, "La policía y el gobierno no pueden robar al banco.");
	if(BankRobbery) return SendClientMessage(playerid, COLOR_GREY, "Un robo de banco ya está siendo configurado.");
	if(BankRobberyTime)
	{
		format(string, sizeof(string), "Usted necesita esperar %d más horas antes de robar el banco de nuevo.", BankRobberyTime);
		SendClientMessage(playerid, COLOR_GREY, string);
	    return 1;
	}
	foreach(Player, i)
	{
	    if(PlayerInfo[i][pFac] == 1 ) cops ++;
	    if(PlayerInfo[i][pFac] == 6) cops ++;
	}
	if(cops < 2) return SendClientMessage(playerid, COLOR_GREY, "No hay suficientes fuerzas de la ley para iniciar un robo.");
	BankRobbery = 1;
	format(BankRobbers[0], MAX_PLAYER_NAME, "%s", RPN(playerid));
	BankRobber[playerid] = 1;
	BankRobbersCount = 1;
	SendClientMessage(playerid, COLOR_LIGHTGREEN, " Otros jugadores pueden unirse usando /unirserobo, charlar con ladrones usando /ro.");
	TextDrawShowForPlayer(playerid, Textdraw0);
	TextDrawSetString(Textdraw1, BankRobbers[0]);
	TextDrawShowForPlayer(playerid, Textdraw1);
	return 1;
}

CMD:unirserobo(playerid, params[])
{
	new string[128];
    if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(IsACop(playerid) || IsAGov(playerid)) return SendClientMessage(playerid, COLOR_GREY, "La policía y el gobierno no pueden robar al banco.");
	if(!BankRobbery) return SendClientMessage(playerid, COLOR_GREY, "No hay ningún robo de banco que se está configurando.");
	if(BankRobber[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Usted ya está robando el banco.");
	if(RobberyRequest[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Usted ya ha solicitado unirse al robo.");
	if(RobberyStarted) return SendClientMessage(playerid, COLOR_GREY, "El robo del banco ya ha comenzado.");
	SendClientMessage(playerid, COLOR_LIGHTGREEN, " Usted ha solicitado unirse al robo, espere a que el organizador de robo tome una acción.");
