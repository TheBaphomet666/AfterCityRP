CMD:descontratar(playerid, params[])
{
	new playerb, string[128], idx = PlayerInfo[playerb][pFac];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pFacRank]!=10 && PlayerInfo[playerid][pFacRank]!=9) return SendClientMessage(playerid, COLOR_GREY, "No eres un líder de faccion.");
	if(sscanf(params, "u", playerb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /descontratar [playerid]");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(PlayerInfo[playerb][pFac] != PlayerInfo[playerid][pFac]) return SendClientMessage(playerid, COLOR_GREY, "El jugador no está en tu facción.");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes desinvitarte.");
	FacInfo[idx][facMembers]--;
	if(PlayerInfo[playerb][pFacLeader])
	{
		format(FacInfo[idx][facLeader], MAX_PLAYER_NAME, "None");
	}
	PlayerInfo[playerb][pFac] = 0;
	PlayerInfo[playerb][pFacRank] = 0;
	PlayerInfo[playerb][pFacLeader] = 0;
	PlayerInfo[playerb][pFacDuty] = 0;
	SetPlayerColor(playerb, TRANSPARENT_WHITE);
	format(string, sizeof(string), " Has expulsado %s de la facción {33AA33}%s {33CCFF}.", RPN(playerb), RPFNEx(playerid));
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " %s te ha expulsado de la facción {33AA33}%s {33CCFF}, ahora eres un civil.", RPN(playerid), RPFNEx(playerid));
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	return 1;
}

CMD:limpiarfac(playerid, params[])
{
	new playerb, string[128], idx = PlayerInfo[playerb][pFac];
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(PlayerInfo[playerid][pAdmin] < 4 && !PlayerInfo[playerid][pFacMod]) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
