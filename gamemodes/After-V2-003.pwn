CMD:mejorarnegocio(playerid, params[])
{
	new string[128], idx = PlayerInfo[playerid][pBiz];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(!PlayerInfo[playerid][pBiz]) return SendClientMessage(playerid, COLOR_GREY, "No posees un negocio.");
    if(GetPlayerVirtualWorld(playerid)-100 != idx) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de tu negocio.");
	if(sscanf(params, "s[8]", params)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /mejorarnegocio [confirmar]");
	if(BizInfo[idx][bLevel] >= 5) return SendClientMessage(playerid, COLOR_GREY, "Tu negocios es del mayor nivel posible.");
	if(!strcmp(params, "confirmar", true))
	{
	    // L1: 0 | L2: 100 | L3: 300 | L4: 700 | L5: 1200
	    if(strval(RPBL(playerid)) == 1 && RPBS(playerid) >= 100) {BizInfo[idx][bLevel]++; SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has mejorado tu negocio al nivel 2.");}
	    else if(strval(RPBL(playerid)) == 2 && RPBS(playerid) >= 300) {BizInfo[idx][bLevel]++; SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has mejorado tu negocio al nivel 3.");}
	    else if(strval(RPBL(playerid)) == 3 && RPBS(playerid) >= 700) {BizInfo[idx][bLevel]++; SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has mejorado tu negocio al nivel 4.");}
	    else if(strval(RPBL(playerid)) == 4 && RPBS(playerid) >= 1200) {BizInfo[idx][bLevel]++; SendClientMessage(playerid, COLOR_LIGHTBLUE, " Has mejorado tu negocio al nivel 5.");}
	    else
		{
		    if(strval(RPBL(playerid)) == 1) format(string, sizeof(string), "Tu negocio necesita vender %d mas productos para mejorarse.", 100 -RPBS(playerid));
		    else if(strval(RPBL(playerid)) == 2) format(string, sizeof(string), "Tu negocio necesita vender %d mas productos para mejorarse.", 300 -RPBS(playerid));
		    else if(strval(RPBL(playerid)) == 3) format(string, sizeof(string), "Tu negocio necesita vender %d mas productos para mejorarse.", 700 -RPBS(playerid));
		    else if(strval(RPBL(playerid)) == 4) format(string, sizeof(string), "Tu negocio necesita vender %d mas productos para mejorarse.", 1200 -RPBS(playerid));
			SendClientMessage(playerid, COLOR_GREY, string);
		}
	}
	return 1;
}

CMD:boveda(playerid, params[])
{
	new value, string[128];
	new idx = PlayerInfo[playerid][pBiz];
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
    if(!PlayerInfo[playerid][pBiz] && !PlayerInfo[playerid][pVBiz]) return SendClientMessage(playerid, COLOR_GREY, "No posee un Negocios.");
    if(GetPlayerVirtualWorld(playerid)-100 != idx && !PlayerInfo[playerid][pVBiz]) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de un negocio.");
    if(GetPlayerVirtualWorld(playerid)-100 != idx && PlayerInfo[playerid][pVBiz])
	{
		idx = PlayerInfo[playerid][pVBiz];
	    if(GetPlayerVirtualWorld(playerid)-100 != idx) return SendClientMessage(playerid, COLOR_GREY, "Usted no está dentro de su negocio.");
	}
	if(sscanf(params, "s[32]", params))
	{
		SendClientMessage(playerid, COLOR_WHITE, "USO: /boveda [opcion]");
		SendClientMessage(playerid, COLOR_GREY, "Opciones: sacar | depositar");
		return 1;
	}
	if(!strcmp(params, "sacar", false, 4))
	{
     if(sscanf(params, "s[32]i", params, value))
	    {
	        SendClientMessage(playerid, COLOR_WHITE, "USO: /boveda sacar [cantidad]");
	        format(string, sizeof(string), "Dinero actual de la caja fuerte: $%d", BizInfo[idx][bMoney]);
	        SendClientMessage(playerid, COLOR_GREY, string);
			return 1;
	    }
	    if(value > BizInfo[idx][bMoney]) return SendClientMessage(playerid, COLOR_GREY, "La bóveda no tiene tando dinero.");
	    BizInfo[idx][bMoney] -= value;
	    GiveDodMoney(playerid, value);
	    format(string, sizeof(string), "* %s ha sacado dinero de su bóveda de negocios.", RPN(playerid));
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
		format(string, sizeof(string), " Usted ha sacado $%d de su bóveda de negocios, cantidad actual: $%d", value, BizInfo[idx][bMoney]);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	}
	else if(!strcmp(params, "depositar", false, 8))
	{
	    if(sscanf(params, "s[32]i", params, value))
	    {
	        SendClientMessage(playerid, COLOR_WHITE, "USO: /boveda depositar [cantidad]");
	        format(string, sizeof(string), "Dinero actual de la caja fuerte: $%d", BizInfo[idx][bMoney]);
	        SendClientMessage(playerid, COLOR_GREY, string);
			return 1;
	    }
	    if(value > PlayerInfo[playerid][pMoney]) return SendClientMessage(playerid, COLOR_GREY, "No tienes mucho dinero contigo.");
	    BizInfo[idx][bMoney] += value;
	    GiveDodMoney(playerid, -value);
	    format(string, sizeof(string), "* %s ha depositado dinero en su bóveda de negocios.", RPN(playerid));
		SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
		format(string, sizeof(string), " Usted ha depositado $%d en su bóveda de negocios, cantidad actual: $%d", value, BizInfo[idx][bMoney]);
		SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	}
	return 1;
}

