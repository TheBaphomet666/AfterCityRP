	format(string, sizeof(string), "[BANCO]: %s te ha transferido $%d, tu nuevo balance es: $%d.", RPN(playerid), amount, PlayerInfo[playerb][pBank]);
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), "%s ha transferido a %s $%d", RPN(playerid), RPN(playerb), amount);
	Log("logs/pay.log", string);
	return 1;
}

