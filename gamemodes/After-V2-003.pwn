CMD:pagar(playerid, params[])
{
	new string[128], playerb, amount;
   	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
	if(sscanf(params, "ui", playerb, amount)) return SendClientMessage(playerid, COLOR_WHITE, "Utiliza /pagar [ID] [cantidad]");
	if(amount <= 0) return SendClientMessage(playerid, COLOR_GREY, "Cantidad de dinero invalida.");
	if(amount > 1000 && PlayerInfo[playerid][pLevel] < 3) return SendClientMessage(playerid, COLOR_GREY, "Necesitas ser nivel 3 para pagar mas de $1000.");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes pagarte a ti mismo.");
	if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(PlayerInfo[playerid][pLevel] < 2) return SendClientMessage(playerid, COLOR_GREY, "Necesitas ser nivel 2 para darle dinero a alguien.");
	if(!IsPlayerNearPlayer(playerid, playerb, 2)) return SendClientMessage(playerid, COLOR_GREY, "Estas muy lejos del usuario.");
	if(PlayerInfo[playerid][pMoney] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes el dinero suficiente ahora mismo.");
	GiveDodMoney(playerid, -amount);
	GiveDodMoney(playerb, amount);
	format(string, sizeof(string), "* %s saco su cartera y le dio dinero a %s.", RPN(playerid), RPN(playerb), amount);
	SendNearbyMessage(playerid, 15, string, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE, COLOR_PURPLE);
	format(string, sizeof(string), " Le pagaste $%d a %s .", amount,RPN(playerb));
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " %s te pago $%d.", RPN(playerid), amount);
	SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	/*format(ircMsg, sizeof(ircMsg), "02[MONEY] [%d] 07%s has paid %s  %d$", playerid, RPN(playerid), RPN(playerb), amount);
	IRC_GroupSay(groupID, IRC_CHANNEL, ircMsg); */
	format(string, sizeof(string), "%s le pago $%d a %s.",RPN(playerid), amount,RPN(playerb));
	Log("logs/pay.log", string);
	return 1;
}

