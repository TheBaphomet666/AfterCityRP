	PlayerInfo[playerid][pBank] -= amount;
	PlayerInfo[playerb][pBank] += amount;
	format(string, sizeof(string), " Has transferido $%d a %s, tu nuevo balance es: $%d.", amount, RPN(playerb), PlayerInfo[playerid][pBank]);
