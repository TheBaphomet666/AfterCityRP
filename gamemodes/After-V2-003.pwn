CMD:venderhierba(playerid, params[])
{
	new playerb, string[128], amount, price, maxweed;
	if(!IsPlayerLoggedIn(playerid) || PlayerInfo[playerid][pAsshole] == 1) return SendClientMessage(playerid, COLOR_GREY, "No estas autorizado para utilizar este comando.");
   	if(PlayerInfo[playerid][pJob] != JOB_DDEALER && PlayerInfo[playerid][pVIPJob] != JOB_DDEALER) return SendClientMessage(playerid, COLOR_GREY, "Usted no es un Distribuidor de Medicamentos.");
	if(sscanf(params, "uii", playerb, amount, price)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /venderhierba [playerid] [cantidad] [precio]");
    if(!IsPlayerLoggedIn(playerb)) return SendClientMessage(playerid, COLOR_GREY, "Id de jugador invalido.");
	if(!IsPlayerNearPlayer(playerid, playerb, 2)) return SendClientMessage(playerid, COLOR_GREY, "Estás muy lejos de ese jugador.");
	if(playerid == playerb) return SendClientMessage(playerid, COLOR_GREY, "No puedes vender hierba a ti mismo.");
	if(WeedSeller[playerb] == playerid) SendClientMessage(playerid, COLOR_GREY, "Ya has ofrecido a este jugador comprar hierba.");
	if(PlayerInfo[playerid][pWeed] < amount) return SendClientMessage(playerid, COLOR_GREY, "No tienes esa cantidad de hierba en ti.");
	if(strval(RPJL(playerb, JOB_DDEALER)) == 1) maxweed = 10;
   	else if(strval(RPJL(playerb, JOB_DDEALER)) == 2) maxweed = 15;
   	else if(strval(RPJL(playerb, JOB_DDEALER)) == 3) maxweed = 20;
   	else if(strval(RPJL(playerb, JOB_DDEALER)) == 4) maxweed = 25;
   	else if(strval(RPJL(playerb, JOB_DDEALER)) == 5) maxweed = 30;
	if(PlayerInfo[playerb][pWeed] + amount > maxweed) return SendClientMessage(playerid, COLOR_GREY, "El jugador no puede sostener esa cantidad de hierba en el momento.");
	WeedSeller[playerb] = playerid;
	WeedPrice[playerid][playerb] = price;
	WeedAmount[playerid][playerb] = amount;
	format(string, sizeof(string), " Has ofrecido% s%d gramos de hierba por $%d.", RPN(playerb), amount, price);
	SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
	format(string, sizeof(string), " %s has offered you %d grams of weed for $%d. (/aceptar weed) (/deny weed)", RPN(playerb), amount, price);
    SendClientMessage(playerb, COLOR_LIGHTBLUE, string);
	return 1;
}

