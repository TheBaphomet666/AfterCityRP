		return 1;
	}
	// Checking for character ban
	if(dini_Int(file, "pBanned") == 1)
	{
	    new Banname[64];
		format(Banname, sizeof(Banname), "%sbanned",RPN(playerid));
	    SetPlayerName(playerid, Banname);
	    SendClientMessage(playerid, COLOR_LIGHTRED, "Administración: {FFFFFF}Estas benado de este servidor. (Reclama tu desbaneo en aftercityrp.xyz)");
	    AddBan(playerid);
	    Kick(playerid);
	    return 1;
	}
	return 1;
}

